	<!-- MANGO -->
		<ThingDef ParentName="VG_FruitTreeBase">
			<defName>PlantMangoTree</defName>
			<label>mango tree</label>
			<description>A juicy stone fruit from tropical trees. The fruits may be somewhat round, oval, or kidney-shaped. It's skin is leather-like, waxy, smooth, and fragrant, with color ranging from green to yellow, yellow-orange, yellow-red, or blushed with various shades of red, pink or yellow when fully ripe.</description>
			<graphicData>
			<texPath>Things/Fruit/MangoTree</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			</graphicData>
			<plant>
			<harvestedThingDef>RawMango</harvestedThingDef>
			<growDays>18</growDays>
			<harvestAfterGrowth>0.30</harvestAfterGrowth>
			<immatureGraphicPath>Things/Immature/MangoTree</immatureGraphicPath>
				<purpose>Food</purpose>
			</plant>
		</ThingDef>
	<!-- AVOCADO -->
		<ThingDef ParentName="VG_FruitTreeBase">
			<defName>PlantAvocadoTree</defName>
			<label>avocado tree</label>
			<description>A fast growing tree that produces avocado fruit, Avocado is a single-seeded berry with a tough skin. It is a pear-shaped fruit and has a large central seed.</description>
			<graphicData>
			<texPath>Things/Fruit/TreeAvocado</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			</graphicData>
			<plant>

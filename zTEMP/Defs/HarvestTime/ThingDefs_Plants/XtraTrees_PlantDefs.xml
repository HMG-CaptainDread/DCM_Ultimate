<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- <></> -->
	<!--  -->


	<!-- TREES -->
		<!-- XTRA TREE BASE -->
			<ThingDef ParentName="TreeBase" Name="VG_XtraTreeBase" Abstract="True">
				<plant>
				<mustBeWildToSow>false</mustBeWildToSow>
				</plant>
			</ThingDef>
		<!-- JAPANESE MAPLE -->
			<ThingDef ParentName="VG_XtraTreeBase">
				<defName>PlantTreeJapaneseMaple</defName>
				<label>japanese maple tree</label>
				<description>These beautiful trees have branches that twist and turn unpredictably and brilliant orange and red foliage that draws the eye.</description>
				<graphicData>
				<texPath>Things/Tree/TreeJapaneseMaple</texPath>
				<graphicClass>Graphic_Random</graphicClass>
				<shadowData>
					<volume>(0.15, 0.3, 0.1)</volume>
				</shadowData>
				<shaderParameters>
					<_FallColorSource>(0.576, 0.235, 0.160)</_FallColorSource>
					<_FallColorDestination>/Other/TreeFallGradient</_FallColorDestination>
					<_FallTransitionRange>(0.02, 0.10)</_FallTransitionRange>
					<_FallBehaviorEnabled>1</_FallBehaviorEnabled>
				</shaderParameters>
				</graphicData>
				<statBases>
				<Beauty>10</Beauty>
				<BeautyOutdoors>10</BeautyOutdoors>
				<Nutrition>1</Nutrition>
				</statBases>
				<plant>
				<leaflessGraphicPath>Things/Tree/Tree_Leafless/TreeJapaneseMaple_Leafless</leaflessGraphicPath>
				<growDays>18</growDays>
				<harvestWork>700</harvestWork>
				<harvestYield>20</harvestYield>
				<sowResearchPrerequisites>
					<li>TreeSowing</li>
				</sowResearchPrerequisites>
				<sowTags>
					<li>Ground</li>
				</sowTags>
				</plant>
			</ThingDef>
		<!-- CHERRY BLOSSOM -->
			<ThingDef ParentName="VG_XtraTreeBase">
				<defName>PlantTreeCherryBlossom</defName>
				<label>cherry blossom tree</label>
				<description>Cherry blossom trees are not just a good source of building materials, they are also one of the most beautiful trees to have in your lawn.</description>
				<graphicData>
				<texPath>Things/Tree/TreeCherryBlossom</texPath>
				<graphicClass>Graphic_Random</graphicClass>
				<shadowData>
					<volume>(0.15, 0.3, 0.1)</volume>
				</shadowData>
					<shaderParameters>
					<_FallColorSource>(1, 0.682, 0.717)</_FallColorSource>
					<_FallColorDestination>/Other/TreeFallGradient</_FallColorDestination>
					<_FallTransitionRange>(0.02, 0.10)</_FallTransitionRange>
					<_FallBehaviorEnabled>1</_FallBehaviorEnabled>
				</shaderParameters>
				</graphicData>
				<statBases>
				<Beauty>8</Beauty>
				<BeautyOutdoors>8</BeautyOutdoors>
				<Nutrition>1</Nutrition>
				</statBases>
				<plant>
				<leaflessGraphicPath>Things/Tree/Tree_Leafless/TreeCherryBlossom_Leafless</leaflessGraphicPath>
				<growDays>20</growDays>
				<harvestWork>700</harvestWork>
				<harvestYield>30</harvestYield>
				<sowResearchPrerequisites>
					<li>TreeSowing</li>
				</sowResearchPrerequisites>
					<sowTags>
					<li>Ground</li>
				</sowTags>
				</plant>
			</ThingDef>
		<!-- CAMELLIA -->
			<ThingDef ParentName="VG_XtraTreeBase">
				<defName>PlantTreeCamellia</defName>
				<label>camellia tree</label>
				<description>This small ornamental tree has beautiful flowers that have been known to stay healthy even through tough winter conditions.</description>
				<graphicData>
				<texPath>Things/Tree/TreeCamellia</texPath>
				<graphicClass>Graphic_Random</graphicClass>
				<shadowData>
					<volume>(0.15, 0.3, 0.1)</volume>
				</shadowData>
					<shaderParameters>
					<_FallColorSource>(0.360, 0.388, 0.180)</_FallColorSource>
					<_FallColorDestination>/Other/TreeFallGradient</_FallColorDestination>
					<_FallTransitionRange>(0.02, 0.10)</_FallTransitionRange>
					<_FallBehaviorEnabled>1</_FallBehaviorEnabled>
				</shaderParameters>
				</graphicData>
				<statBases>
				<Beauty>8</Beauty>
				<BeautyOutdoors>8</BeautyOutdoors>
				<Nutrition>1</Nutrition>
				</statBases>
				<plant>
					<leaflessGraphicPath>Things/Plant/TreeBirch_Leafless</leaflessGraphicPath>
					<growDays>20</growDays>
					<harvestWork>700</harvestWork>
					<harvestYield>30</harvestYield>
					<sowResearchPrerequisites>
						<li>TreeSowing</li>
					</sowResearchPrerequisites>
				</plant>
			</ThingDef>
		<!-- ACACIA -->
			<ThingDef ParentName="VG_XtraTreeBase">
				<defName>PlantTreeAcacia</defName>
				<label>acacia tree</label>
				<description>Acacia trees are hardy and can grow in the toughest desert environments. They don't provide much wood and they have sharp thorns too.</description>
				<graphicData>
				<texPath>Things/Tree/TreeAcacia</texPath>
				<graphicClass>Graphic_Random</graphicClass>
				<shadowData>
					<volume>(0.15, 0.3, 0.1)</volume>
				</shadowData>
				</graphicData>
				<statBases>
				<Beauty>3</Beauty>
				<BeautyOutdoors>3</BeautyOutdoors>
				<Nutrition>1</Nutrition>
				</statBases>
					<plant>
						<leaflessGraphicPath>Things/Plant/TreeBirch_Leafless</leaflessGraphicPath>
						<growDays>15</growDays>
						<harvestWork>700</harvestWork>
						<harvestYield>12</harvestYield>
						<sowResearchPrerequisites>
							<li>TreeSowing</li>
						</sowResearchPrerequisites>
					</plant>
			</ThingDef>

	<!-- TOBACCO TOLERANCE -->
		<HediffDef ParentName="DrugToleranceBase">
			<defName>TobaccoTolerance</defName>
			<label>tobacco tolerance</label>
			<description>Because of an tobacco addiction, this person needs to regularly consume more the drug to get that fix.</description>

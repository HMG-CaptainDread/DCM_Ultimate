	<!-- HEMP CLOTH -->
		<ThingDef ParentName="ResourceBase">
			<defName>HempCloth</defName>
			<label>Hemp</label>
			<description>Cloth woven from the stuff those guys are smoking who decided that wearing this was a good idea?.</description>
			<graphicData>
			<texPath>Things/Item/HempCloth</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(1,100,14)</color>
			</graphicData>
			<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>3</MarketValue>
			<Flammability>1.5</Flammability>
			<DeteriorationRate>3</DeteriorationRate>
			<Mass>0.028</Mass>
			<StuffPower_Armor_Sharp>0.45</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.40</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>22</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>18</StuffPower_Insulation_Heat>
			</statBases>
			<burnableByRecipe>true</burnableByRecipe>
			<healthAffectsPrice>false</healthAffectsPrice>
			<smeltable>true</smeltable>
			<stuffProps>
			<statFactors>
				<Flammability>1</Flammability>
			</statFactors>
			<stuffAdjective>hemp</stuffAdjective>
			<categories>
				<li>Fabric</li>
			</categories>
			<allowColorGenerators>false</allowColorGenerators>
			</stuffProps>
			<thingCategories>
			<li>Textiles</li>
			</thingCategories>
			<comps>
			<li>
				<compClass>CompColorable</compClass>
			</li>
			</comps>
		</ThingDef>

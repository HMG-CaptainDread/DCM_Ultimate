	<!-- BIOFERRITE TREE -->
		<ThingDef ParentName="TMM_PlantBase">
			<defName>TMM_BioferriteTree</defName>
			<label>Bioferrite Tree</label>
			<description>A strange crystal-like plant whose unique DNA structure produces bioferrite. This strange plant dies under sunlight, but thrives in dark places.</description>
			<graphicData>
				<texPath>Modules/HarvestTime/Plants/Mature/ResourcePlants/Bioferrite</texPath>
			</graphicData>
			<plant>
				<neverBlightable>true</neverBlightable>
				<harvestedThingDef>TMM_RawBioferriteLeaf</harvestedThingDef>
				<harvestYield>16</harvestYield>
				<sowMinSkill>8</sowMinSkill>
				<growDays>12</growDays>
				<fertilitySensitivity>1</fertilitySensitivity>
				<growMinGlow>0</growMinGlow>
				<growOptimalGlow>0</growOptimalGlow>
				<cavePlant>true</cavePlant>
				<diesToLight>true</diesToLight>
				<wildOrder>2</wildOrder>
				<purpose>Misc</purpose>
					<sowResearchPrerequisites>
						<li>TMM_HT_AgricultureV</li>
						<li>BioferriteExtraction</li>
					</sowResearchPrerequisites>
				</plant>
		</ThingDef>
</Defs>
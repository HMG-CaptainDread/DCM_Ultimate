			<!-- CRANBERRY -->
				<ThingDef ParentName="PlantBase">
					<defName>FCP_Plant_Cranberry</defName>
					<label>Cranberry</label>
					<description>A tart, red berry cultivated in wasteland farms for its nutritional value and use in preserves. Cranberries are a resilient crop, thriving in moist soil.</description>
					<statBases>
						<MaxHitPoints>65</MaxHitPoints>
						<Nutrition>0.15</Nutrition>
					</statBases>
					<graphicData>
						<texPath>Modules/HarvestTime/Plants/Mature/FCP/Cranberry</texPath>
						<graphicClass>Graphic_Random</graphicClass>
						<shadowData>
							<volume>(0.08, 0.25, 0.08)</volume>
						</shadowData>
					</graphicData>
					<selectable>true</selectable>
					<neverMultiSelect>false</neverMultiSelect>
					<fillPercent>0.08</fillPercent>
					<pathCost>16</pathCost>
					<ingestible />
					<plant>
						<sowTags>
							<li>Ground</li>
						</sowTags>
						<sowResearchPrerequisites>
							<li>FCP_Research_Basic_Agriculture</li>
						</sowResearchPrerequisites>
						<immatureGraphicPath>Modules/HarvestTime/Plants/Immature/FCP/Cranberry</immatureGraphicPath>
						<fertilitySensitivity>0.7</fertilitySensitivity>
						<growDays>13.0</growDays>
						<harvestTag>Standard</harvestTag>
						<harvestAfterGrowth>0.55</harvestAfterGrowth>
						<harvestedThingDef>FCP_Raw_Cranberry</harvestedThingDef>
						<harvestYield>6</harvestYield>
						<harvestWork>45</harvestWork>
						<visualSizeRange>0.6~1.0</visualSizeRange>
						<topWindExposure>0.05</topWindExposure>
						<purpose>Food</purpose>
						<dieFromToxicFallout>false</dieFromToxicFallout>
					</plant>
				</ThingDef>

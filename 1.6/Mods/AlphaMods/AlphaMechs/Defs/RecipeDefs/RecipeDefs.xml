	<!-- HYPER LINKAGE CHIP -->
		<RecipeDef ParentName="TMM_SubcoreRecipeBase">
			<defName>TMM_CraftHyperLinkageChip</defName>
			<label>make hyper linkage chip</label>
			<description>Make a beamcasting-band synchronization micro-organ. It is necessary to produce certain advanced types of mechanoids and equipment.</description>
			<jobString>Making a hyper linkage chip.</jobString>
			<researchPrerequisite>AM_VoidLinkConnectivity</researchPrerequisite>
			<displayPriority>13</displayPriority>
			<ingredients>
				<li>
					<filter>
						<thingDefs>
							<li>NanostructuringChip</li>
						</thingDefs>
					</filter>
					<count>1</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Plasteel</li>
						</thingDefs>
					</filter>
					<count>50</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Uranium</li>
						</thingDefs>
					</filter>
					<count>15</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>ComponentSpacer</li>
						</thingDefs>
					</filter>
					<count>2</count>
				</li>
			</ingredients>
			<fixedIngredientFilter>
				<thingDefs>
					<li>NanostructuringChip</li>
					<li>Plasteel</li>
					<li>Uranium</li>
					<li>ComponentSpacer</li>
				</thingDefs>
			</fixedIngredientFilter>
			<defaultIngredientFilter>
				<thingDefs>
					<li>NanostructuringChip</li>
					<li>Plasteel</li>
					<li>Uranium</li>
					<li>ComponentSpacer</li>
				</thingDefs>
			</defaultIngredientFilter>
			<products>
				<AM_HyperLinkageChip>1</AM_HyperLinkageChip>
			</products>
		</RecipeDef>

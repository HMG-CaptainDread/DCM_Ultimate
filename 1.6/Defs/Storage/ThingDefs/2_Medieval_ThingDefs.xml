	<!-- ANIMAL TROUGH -->
		<ThingDef ParentName="AdaptiveStorageBase">
			<defName>TMM_AnimalTrough</defName>
			<label>animal trough</label>
			<graphicData>
				<texPath>Modules/Storage/Building/2_Medieval/AnimalTrough/AnimalTrough_Empty/AnimalTrough</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
				<drawSize>(3,3)</drawSize>
				<drawRotated>false</drawRotated>
			</graphicData>
			<description>A specialized storage container designed to hold food for feeding your colony animals. Items stored inside will not deteriorate over time and have no impact on the beauty of their surroundings. \n\n&lt;color=#E5E54C&gt;Storage stats:&lt;/color&gt; Stack size total: 18 Stack per tile: 9 Allowed items: Hay, Kibble</description>
			<fillPercent>0.20</fillPercent>
			<uiIconScale>1</uiIconScale>
			<size>(2,1)</size>
			<rotatable>true</rotatable>
			<defaultPlacingRot>North</defaultPlacingRot>
			<researchPrerequisites>
				<li>TMM_St_Medieval</li>
			</researchPrerequisites>
			<statBases>
				<MaxHitPoints>225</MaxHitPoints>
				<WorkToBuild>1400</WorkToBuild>
				<Beauty>0</Beauty>
				<Mass>9</Mass>
				<Flammability>1</Flammability>
			</statBases>
			<stuffCategories>
				<li>Woody</li>
				<li>Metallic</li>
			</stuffCategories>
			<costStuffCount>60</costStuffCount>
			<building>
				<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
				<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
				<maxItemsInCell>9</maxItemsInCell>
				<fixedStorageSettings>
					<filter>
						<specialFiltersToAllow>
							<li MayRequire="ludeon.rimworld.ideology">AllowVegetarian</li>
						</specialFiltersToAllow>
						<disallowNotEverStorable>true</disallowNotEverStorable>
						<thingDefs>
							<li>Hay</li>
							<li>Kibble</li>
							<li MayRequire="vanillaexpanded.vplantsemore,Ludeon.RimWorld.Ideology">VCE_RawMoss</li>
						</thingDefs>
					</filter>
				</fixedStorageSettings>
				<defaultStorageSettings>
					<priority>Preferred</priority>
					<filter>
						<disallowNotEverStorable>true</disallowNotEverStorable>
						<thingDefs>
							<li>Hay</li>
							<li>Kibble</li>
							<li MayRequire="vanillaexpanded.vplantsemore,Ludeon.RimWorld.Ideology">VCE_RawMoss</li>
						</thingDefs>
					</filter>
				</defaultStorageSettings>
				<paintable>true</paintable>
			</building>
			<minifiedDef>MinifiedThing</minifiedDef>
			<thingCategories>
				<li>BuildingsFurniture</li>
			</thingCategories>
		</ThingDef>

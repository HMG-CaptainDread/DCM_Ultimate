	<!-- STONE POTS -->
		<!-- COVERED STONE POT -->
			<ThingDef ParentName="AdaptiveStorageBase">
				<defName>TMM_StonePot_Covered</defName>
				<label>covered pot</label>
				<graphicData>
					<texPath>Modules/Storage/Building/1_Neolithic/StonePots/StonePot_Covered/StonePot_Covered</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(2,2)</drawSize>
					<drawRotated>false</drawRotated>
				</graphicData>
				<description>The most basic of all storage vessels, the pot. It's capable of holding foodstuffs and raw resources. Items stored inside will never deteriorate and don't affect the beauty of their surroundings.\n\nRotate to change appearance.</description>
				<fillPercent>0.20</fillPercent>
				<uiIconPath>Modules/Storage/UI/1_Neolithic/StonePots/StonePot_Covered/StonePot_Covered_UI</uiIconPath>
				<uiIconScale>1</uiIconScale>
				<size>(1,1)</size>
				<defaultPlacingRot>North</defaultPlacingRot>
				<researchPrerequisites>
					<li>TMM_St_Neolithic</li>
				</researchPrerequisites>
				<statBases>
					<MaxHitPoints>60</MaxHitPoints>
					<WorkToBuild>600</WorkToBuild>
					<Beauty>0</Beauty>
					<Mass>4</Mass>
					<Flammability>0</Flammability>
				</statBases>
				<stuffCategories>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>15</costStuffCount>
				<building>
					<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
					<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
					<maxItemsInCell>3</maxItemsInCell>
					<fixedStorageSettings>
						<filter>
							<disallowNotEverStorable>true</disallowNotEverStorable>
							<categories>
								<li>Foods</li>
								<li>ResourcesRaw</li>
							</categories>
							<disallowedCategories>
								<li>FoodMeals</li>
							</disallowedCategories>
						</filter>
					</fixedStorageSettings>
					<paintable>true</paintable>
				</building>
				<minifiedDef>MinifiedThing</minifiedDef>
				<thingCategories>
					<li>BuildingsFurniture</li>
				</thingCategories>
			</ThingDef>

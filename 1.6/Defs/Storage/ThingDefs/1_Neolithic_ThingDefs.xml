	<!-- STORAGE PIT -->
		<ThingDef ParentName="AdaptiveStorageBase">
			<defName>TMM_StoragePit</defName>
			<label>storage pit</label>
			<description>A dirty hole in the ground that can store some items. Keeps perishable items slightly cooler underground, effective at around 65°F/18°C. Items stored inside will never deteriorate and won't affect the beauty of their surroundings.\n\nOccupies a 1x1 area underground.\n\nTotal Capacity: 25 Stacks</description>
			<graphicData>
				<texPath>Modules/Storage/Building/1_Neolithic/StoragePit/StoragePit</texPath>
				<graphicClass>Graphic_Single</graphicClass>
				<drawSize>(1.75,1.75)</drawSize>
				<drawRotated>false</drawRotated>
				<shaderType>CutoutComplex</shaderType>
			</graphicData>
			<uiIconPath>Modules/Storage/UI/1_Neolithic/StoragePit/StoragePit_UI</uiIconPath>
			<uiIconScale>1</uiIconScale>
			<size>(1,1)</size>
			<rotatable>false</rotatable>
			<statBases>
				<Beauty>-6</Beauty>
				<MaxHitPoints>1000</MaxHitPoints>
				<WorkToBuild>15000</WorkToBuild>
				<Flammability>0.0</Flammability>
				<Mass>4000</Mass>
			</statBases>
			<stuffCategories>
				<li>Woody</li>
			</stuffCategories>
			<costStuffCount>25</costStuffCount>
			<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
			<building>
				<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
				<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
				<maxItemsInCell>25</maxItemsInCell>
			</building>
			<minifiedDef>MinifiedThing</minifiedDef>
			<thingCategories>
				<li>BuildingsFurniture</li>
			</thingCategories>
			<modExtensions>
				<li Class="AdaptiveStorage.Extension">
					<temperature>
						<coolingOffset>10</coolingOffset>
					</temperature>
				</li>
			</modExtensions>
		</ThingDef>

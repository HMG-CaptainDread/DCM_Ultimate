	<!-- LOG PILES -->
		<!-- LARGE LOG PILE -->
			<ThingDef ParentName="AdaptiveStorageBase">
				<defName>TMM_LogPile_Large</defName>
				<label>large log pile spot</label>
				<description>A designated spot to pile wood. Free to build. Deconstructing returns 100% of wood logs stored. Items stored inside will never deteriorate and don't affect the beauty of their surroundings.\n\nTotal Capacity: 12 Stacks</description>
				<graphicData>
					<texPath>Modules/Storage/Building/1_Neolithic/LogPiles/LogPile_Large/LogPile_Large_F1</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(3.8,2)</drawSize>
					<drawRotated>false</drawRotated>
					<drawOffsetNorth>0,0,0.125</drawOffsetNorth>
					<drawOffsetSouth>0,0,-0.125</drawOffsetSouth>
					<drawOffsetEast>0,0.125,0</drawOffsetEast>
					<drawOffsetWest>0,-0.125,0</drawOffsetWest>
				</graphicData>
				<fillPercent>0.7</fillPercent>
				<uiIconPath>Modules/Storage/UI/1_Neolithic/LogPiles/LogPile_Large/LogPile_Large_UI</uiIconPath>
				<uiIconScale>1.35</uiIconScale>
				<size>(3,1)</size>
				<rotatable>true</rotatable>
				<defaultPlacingRot>South</defaultPlacingRot>
				<statBases>
					<Beauty>0</Beauty>
					<MaxHitPoints>300</MaxHitPoints>
					<WorkToBuild>0</WorkToBuild>
					<Flammability>5.0</Flammability>
					<Mass>0</Mass>
				</statBases>
				<stuffCategories>
					<li>Woody</li>
				</stuffCategories>
				<costStuffCount>0</costStuffCount>
				<building>
					<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
					<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
					<maxItemsInCell>4</maxItemsInCell>
					<defaultStorageSettings>
						<priority>Important</priority>
						<filter>
							<thingDefs>
								<li>WoodLog</li>
								<li MayRequire="dankpyon.medieval.overhaul">DankPyon_RawWood</li>
								<li MayRequire="dankpyon.medieval.overhaul">DankPyon_RawDarkWood</li>
							</thingDefs>
						</filter>
					</defaultStorageSettings>
					<fixedStorageSettings>
						<filter>
							<thingDefs>
								<li>WoodLog</li>
								<li MayRequire="dankpyon.medieval.overhaul">DankPyon_RawWood</li>
								<li MayRequire="dankpyon.medieval.overhaul">DankPyon_RawDarkWood</li>
							</thingDefs>
						</filter>
					</fixedStorageSettings>
				</building>
				<minifiedDef>MinifiedThing</minifiedDef>
				<thingCategories>
					<li>BuildingsFurniture</li>
				</thingCategories>
			</ThingDef>

		<!-- CARGO HOLD (MEDIUM REFRIGERATED) -->
			<ThingDef ParentName="AdaptiveStorageBase">
				<defName>TMM_CargoHold_Medium_Refrigerated</defName>
				<label>refrigerated cargo hold</label>

	<!-- CARGO HOLDS -->
		<!-- CARGO HOLD (LARGE) -->
			<ThingDef MayRequire="Ludeon.Rimworld.Odyssey" ParentName="AdaptiveStorageBase">
				<defName>TMM_CargoHold_Large</defName>
				<label>large cargo hold</label>
				<graphicData>
					<texPath>Modules/Storage/Building/5_Ultra/CargoHolds/CargoHold_Large/CargoHold_Large</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(1.9,1.9)</drawSize>
					<drawRotated>false</drawRotated>
					<allowFlip>false</allowFlip>
					<shaderType>CutoutComplex</shaderType>
				</graphicData>
				<description>A non-refrigerated cargo hold perfect for bulk transport. Keeps your items stowed and safely tucked away under the floor. \n\nCapacity: 24 Stacks</description>
				<researchPrerequisites>
					<li>TMM_St_Ultra</li>
				</researchPrerequisites>
				<fillPercent>0.1</fillPercent>
				<uiIconPath>Modules/Storage/UI/5_Ultra/CargoHolds/CargoHold_Large/CargoHold_Large_UI</uiIconPath>
				<uiIconScale>1</uiIconScale>
				<size>(2,2)</size>
				<rotatable>true</rotatable>
				<defaultPlacingRot>South</defaultPlacingRot>
				<terrainAffordanceNeeded Inherit="False">Heavy</terrainAffordanceNeeded>
				<statBases>
					<Beauty>0</Beauty>
					<MaxHitPoints>1500</MaxHitPoints>
					<WorkToBuild>25000</WorkToBuild>
					<Flammability>0.0</Flammability>
					<Mass>10000</Mass>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
				</stuffCategories>
				<passability>Standable</passability>
				<pathCost>10</pathCost>
				<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
				<costStuffCount>350</costStuffCount>
				<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
				<building>
					<maxItemsInCell>6</maxItemsInCell>
					<fixedStorageSettings>
						<filter>
							<disallowNotEverStorable>true</disallowNotEverStorable>
							<categories>
								<li>Root</li>
							</categories>
							<disallowedCategories>
								<li>Chunks</li>
								<li>Corpses</li>
								<li>Plants</li>
								<li>Buildings</li>
							</disallowedCategories>
						</filter>
					</fixedStorageSettings>
				</building>
				<minifiedDef />
				<placeWorkers>
					<li>PlaceWorker_NeverAdjacentSameDef</li>
					<li>PlaceWorker_OnSubstructure</li>
				</placeWorkers>
				<altitudeLayer>FloorEmplacement</altitudeLayer>
				<modExtensions>
					<li Class="AdaptiveStorage.Extension">
						<temperature>
							<coolingOffset>10</coolingOffset>
						</temperature>
					</li>
				</modExtensions>
			</ThingDef>

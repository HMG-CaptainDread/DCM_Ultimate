<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- <></> -->
	<!--  -->



	<!-- CORPSE PILE -->
		<ThingDef ParentName="AdaptiveStorageBase">
			<defName>TMM_CorpsePile</defName>
			<label>corpse pile</label>
			<description>A spot that can store up to 12 corpses.</description>
			<altitudeLayer>FloorEmplacement</altitudeLayer>
			<rotatable>true</rotatable>
			<passability>Standable</passability>
			<statBases>
				<WorkToBuild>0</WorkToBuild>
			</statBases>
			<useHitPoints>false</useHitPoints>
			<size>(1,1)</size>
			<surfaceType>Item</surfaceType>
			<graphicData>
				<texPath>Modules/Storage/Building/0_Natural/CorpsePile/CorpsePile</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
				<drawSize>(1,1)</drawSize>
				<color>(155,155,155)</color>
			</graphicData>
			<uiIconScale>1</uiIconScale>
			<uiOrder>950</uiOrder>
			<building>
				<preventDeteriorationOnTop>false</preventDeteriorationOnTop>
				<ignoreStoredThingsBeauty>false</ignoreStoredThingsBeauty>
				<maxItemsInCell>12</maxItemsInCell>
				<fixedStorageSettings>
					<filter>
						<disallowNotEverStorable>true</disallowNotEverStorable>
						<categories>
							<li>Corpses</li>
						</categories>
					</filter>
				</fixedStorageSettings>
				<defaultStorageSettings>
					<priority>Preferred</priority>
					<filter>
						<disallowNotEverStorable>true</disallowNotEverStorable>
						<categories></categories>
					</filter>
				</defaultStorageSettings>
				<paintable>true</paintable>
			</building>
		</ThingDef>
	<!--  -->
</Defs>
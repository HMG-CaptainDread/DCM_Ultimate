<Defs>
	<!-- <></> -->
	<!--  -->



	<!-- BAKING -->
		<!-- BREADS -->
			<!-- LOAF -->
				<PipeSystem.ProcessDef>
					<defName>TMM_BakeBreadLoaf</defName>
					<label>Bake Bread</label>
					<description>Turns bread dough into 6 loaves.</description>
					<labelOverride>Turns bread dough into 6 loaves.</labelOverride>
					<priorityInBillList>0</priorityInBillList>
					<spawnOnInteractionCell>true</spawnOnInteractionCell>
					<useIngredients>true</useIngredients>
					<transfersIngredientList>true</transfersIngredientList>
					<ingredients>
						<li>
							<thing>TMM_DoughBread</thing>
							<countNeeded>1</countNeeded>
						</li>
					</ingredients>
					<ticks>1200</ticks>
					<results>
						<li>
							<thing>TMM_BreadLoaf</thing>
							<count>6</count>
						</li>
					</results>
					<noPowerDestroysProgress>true</noPowerDestroysProgress>
					<noPowerDestroysMessage>VCE_OvenFailurePower</noPowerDestroysMessage>
					<noPowerDestroysInitialWarning>VCE_OvenFailurePowerWarning</noPowerDestroysInitialWarning>
					<autoExtract>false</autoExtract>
				</PipeSystem.ProcessDef>
</Defs>
	<!-- BASES -->
		<!-- SUBCORE BASE -->
			<RecipeDef Name="TMM_SubcoreRecipeBase" Abstract="True">
				<jobString>Making a subcore.</jobString>
				<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
				<effectWorking>Smith</effectWorking>
				<soundWorking>Recipe_Machining</soundWorking>
				<workAmount>3000</workAmount>
				<workSkill>Crafting</workSkill>
				<unfinishedThingDef>UnfinishedSubcore</unfinishedThingDef>
				<mechanitorOnlyRecipe>true</mechanitorOnlyRecipe>
				<recipeUsers>
					<li>SubcoreEncoder</li>
				</recipeUsers>
			</RecipeDef>
		<!-- CHIP BASE -->
			<RecipeDef Name="TMM_ChipRecipeBase" Abstract="True">
				<jobString>Making a chip.</jobString>
				<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
				<effectWorking>Smith</effectWorking>
				<soundWorking>Recipe_Machining</soundWorking>
				<workAmount>3000</workAmount>
				<workSkill>Crafting</workSkill>
				<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
				<mechanitorOnlyRecipe>true</mechanitorOnlyRecipe>
				<recipeUsers>
					<li>SubcoreEncoder</li>
				</recipeUsers>
			</RecipeDef>
		<!-- MISCELLANEOUS BASE -->
			<RecipeDef Name="TMM_MiscellaneousMechRecipeBase" Abstract="True">
				<jobString>Making something.</jobString>
				<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
				<effectWorking>Smith</effectWorking>
				<soundWorking>Recipe_Machining</soundWorking>
				<workAmount>3000</workAmount>
				<workSkill>Crafting</workSkill>
				<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>

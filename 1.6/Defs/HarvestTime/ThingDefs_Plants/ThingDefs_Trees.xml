	<!-- REGULAR TREES -->
		<!-- ACACIA -->
			<ThingDef ParentName="TMM_TreeBase">
				<defName>TMM_AcaciaTree</defName>
				<label>Acacia Tree</label>
				<description>Acacia trees are hardy and can grow in the toughest of arid environments. They don't provide much wood, and have sharp thorns to ward off many would be lumberjacks.</description>
				<graphicData>
					<texPath>Modules/HarvestTime/Plants/Mature/Trees/Acacia</texPath>
					<shadowData>
						<volume>(0.15, 0.3, 0.1)</volume>
					</shadowData>
				</graphicData>
				<statBases>
					<Beauty>3</Beauty>
					<BeautyOutdoors>3</BeautyOutdoors>
				</statBases>
				<plant>
					<leaflessGraphicPath>Things/Plant/TreeBirch_Leafless</leaflessGraphicPath>
					<growDays>15</growDays>
					<harvestWork>700</harvestWork>
					<harvestYield>12</harvestYield>
				</plant>
			</ThingDef>

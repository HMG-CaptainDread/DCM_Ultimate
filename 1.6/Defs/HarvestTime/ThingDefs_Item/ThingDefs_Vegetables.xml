<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- <></> -->
	<!--  -->



	<!-- BASES -->
	<!-- MEATY PLANT BASE -->
		<ThingDef Abstract="True" ParentName="OrganicProductBase" Name="TMM_MeatyPlantBase">
			<graphicData>
				<graphicClass>Graphic_StackCount</graphicClass>
			</graphicData>
			<socialPropernessMatters>true</socialPropernessMatters>
			<statBases>
				<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
				<MarketValue>2</MarketValue>
				<Nutrition>0.05</Nutrition>
			</statBases>
			<ingestible>
				<foodType>Meat</foodType>
				<ingestEffect>EatMeat</ingestEffect>
				<ingestSound>RawMeat_Eat</ingestSound>
				<tasteThought>AteRawFood</tasteThought>
				<specialThoughtDirect>AteRawFood</specialThoughtDirect>
			</ingestible>
			<allowedArchonexusCount>50</allowedArchonexusCount>
			<generateCommonality>0</generateCommonality>
			<thingCategories>
				<li>GardenMeats</li>
			</thingCategories>
			<comps>
				<li Class="CompProperties_Rottable">
					<daysToRotStart>14</daysToRotStart>
					<rotDestroys>true</rotDestroys>
				</li>
			</comps>
		</ThingDef>
	<!-- MUSHROOM -->
		<ThingDef ParentName="TMM_MeatyPlantBase">
			<defName>TMM_RawMushroom</defName>
			<label>Mushroom</label>
			<description>Raw, protein-rich mushrooms. Nice to eat, even when raw.</description>
			<graphicData>
				<texPath>Modules/HarvestTime/Things/NewThings/Vegetable/Mushroom</texPath>
			</graphicData>
			<ingestible>
				<preferability>RawTasty</preferability>
			</ingestible>
		</ThingDef>
	<!-- RED LENTIL -->
		<ThingDef ParentName="TMM_MeatyPlantBase">
			<defName>TMM_RawRedLentil</defName>
			<label>Red Lentil</label>
			<description>Raw, protein-rich lentil. Used in place of meat in recipes.</description>
			<graphicData>
				<texPath>Modules/HarvestTime/Things/NewThings/Vegetable/RedLentil</texPath>
			</graphicData>
			<ingestible>
				<preferability>DesperateOnly</preferability>
			</ingestible>
		</ThingDef>
	<!-- SNOW BEET -->
		<ThingDef ParentName="PlantFoodRawBase">
			<defName>TMM_RawSnowBeet</defName>
			<label>Snow Beet</label>
			<description>Raw snow beet.</description>
			<graphicData>
				<texPath>Modules/HarvestTime/Things/NewThings/Vegetable/SnowBeet</texPath>
				<graphicClass>Graphic_StackCount</graphicClass>
			</graphicData>
			<statBases>
				<MarketValue>1.2</MarketValue>
			</statBases>
			<ingestible>
				<foodType>VegetableOrFruit</foodType>
			</ingestible>
			<comps>
				<li Class="CompProperties_Rottable">
					<daysToRotStart>30</daysToRotStart>
					<rotDestroys>true</rotDestroys>
				</li>
			</comps>
		</ThingDef>
</Defs>
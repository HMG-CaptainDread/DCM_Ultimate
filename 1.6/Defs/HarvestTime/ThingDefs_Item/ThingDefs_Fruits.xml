	<!-- RAW FRUIT BASE -->
		<ThingDef ParentName="PlantFoodRawBase" Name="TMM_RawFruitBase" Abstract="True">
			<soundInteract>Grain_Drop</soundInteract>
			<soundDrop>Grain_Drop</soundDrop>
			<graphicData>
				<graphicClass>Graphic_Random</graphicClass>
			</graphicData>
			<socialPropernessMatters>true</socialPropernessMatters>
			<statBases>
				<MaxHitPoints>100</MaxHitPoints>
				<Flammability>1</Flammability>
				<DeteriorationRate>6</DeteriorationRate>
				<Mass>0.03</Mass>
				<Nutrition>0.08</Nutrition>
				<FoodPoisonChanceFixedHuman>0.01</FoodPoisonChanceFixedHuman>
			</statBases>
			<ingestible>
				<preferability>RawTasty</preferability>
				<tasteThought />
				<ingestEffect>EatVegetarian</ingestEffect>
				<ingestSound>RawVegetable_Eat</ingestSound>
				<foodType>VegetableOrFruit</foodType>
				<joy>0.02</joy>
				<joyKind>Gluttonous</joyKind>
			</ingestible>
			<thingCategories>
				<li MayRequire="VanillaExpanded.VPlantsE">VCE_Fruit</li>
				<li MayRequire="Mlie.RC2.Core">RC2_FruitsRaw</li>
			</thingCategories>
		</ThingDef>

<Defs>
	<!-- <></> -->
	<!--  -->


	<!-- PLANTERS BASES -->
		<!-- PLANTER BASE -->
			<ThingDef ParentName="BuildingBase" Name="TMM_PlanterBase" Abstract="True">
				<thingClass>Building_PlantGrower</thingClass>
				<designatorDropdown>TMM_PlanterBoxGroup</designatorDropdown>
				<pathCostIgnoreRepeat>true</pathCostIgnoreRepeat>
				<altitudeLayer>FloorEmplacement</altitudeLayer>
				<passability>PassThroughOnly</passability>
				<castEdgeShadows>true</castEdgeShadows>
				<fillPercent>0.30</fillPercent>
				<canOverlapZones>false</canOverlapZones>
				<statBases>
					<MaxHitPoints>180</MaxHitPoints>
					<Flammability>1</Flammability>
					<Cleanliness>-3</Cleanliness>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<fertility>0.70</fertility>
				<building>
					<defaultPlantToGrow>Plant_Rice</defaultPlantToGrow>
					<sowTag>Hydroponic</sowTag>
				</building>
				<comps>
					<li Class="CompProperties_Forbiddable"/>
					<li Class="CompProperties_Power">
						<compClass>CompPowerTransmitter</compClass>
						<transmitsPower>false</transmitsPower>
					</li>
				</comps>
				<tickerType>Rare</tickerType>
				<designationCategory>TMM_HarvestTimeDesignator</designationCategory>
			</ThingDef>
		<!-- PLANTER BOX BASE -->
			<ThingDef ParentName="TMM_PlanterBase" Name="TMM_PlanterBoxBase" Abstract="True">
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<damageData>
						<cornerTL>Damage/Corner</cornerTL>
						<cornerTR>Damage/Corner</cornerTR>
						<cornerBL>Damage/Corner</cornerBL>
						<cornerBR>Damage/Corner</cornerBR>
					</damageData>
				</graphicData>
				<pathCost>60</pathCost>
				<statBases>
					<WorkToBuild>2800</WorkToBuild>
				</statBases>
				<costStuffCount>20</costStuffCount>
				<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
			</ThingDef>
		<!-- PLANTER MODULAR BASE -->
			<ThingDef ParentName="TMM_PlanterBase" Name="TMM_ModularPlanterBase" Abstract="True">
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<linkType>Basic</linkType>
					<linkFlags>
						<li>Sandbags</li>
					</linkFlags>
					<damageData>
						<rect>(0.2,0.2,0.6,0.6)</rect>
					</damageData>
				</graphicData>
				<pathCost>40</pathCost>
				<statBases>
					<WorkToBuild>650</WorkToBuild>
				</statBases>
				<costStuffCount>4</costStuffCount>
				<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
			</ThingDef>
	<!-- HYDROPONICS BASES -->
		<!-- HYDROPONICS BASE -->
			<ThingDef ParentName="BuildingBase" Name="TMM_HydroponicsBase" Abstract="True">
				<thingClass>Building_PlantGrower</thingClass>
				<designatorDropdown>TMM_HydroponicsGroup</designatorDropdown>
				<pathCostIgnoreRepeat>true</pathCostIgnoreRepeat>
				<altitudeLayer>FloorEmplacement</altitudeLayer>
				<passability>PassThroughOnly</passability>
				<castEdgeShadows>true</castEdgeShadows>
				<fillPercent>0.30</fillPercent>
				<canOverlapZones>false</canOverlapZones>
				<constructEffect>ConstructMetal</constructEffect>
				<statBases>
					<MaxHitPoints>180</MaxHitPoints>
					<Flammability>1</Flammability>
					<Cleanliness>-3</Cleanliness>
				</statBases>
				<fertility>2.8</fertility>
				<building>
					<defaultPlantToGrow>Plant_Rice</defaultPlantToGrow>
					<sowTag>Hydroponic</sowTag>
				</building>
				<comps>
					<li Class="CompProperties_Forbiddable"/>
					<li Class="CompProperties_Power">
						<compClass>CompPowerTrader</compClass>
						<basePowerConsumption>70</basePowerConsumption>
					</li>
					<li Class="CompProperties_Flickable"/>
					<li Class="CompProperties_Breakdownable"/>
				</comps>
				<researchPrerequisites>
					<li>Hydroponics</li>
				</researchPrerequisites>
				<tickerType>Rare</tickerType>
				<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
				<designationCategory>TMM_HarvestTimeDesignator</designationCategory>
			</ThingDef>
		<!-- HYDROPONICS BASIN BASE -->
			<ThingDef ParentName="TMM_HydroponicsBase" Name="TMM_HydroponicsBasinBase" Abstract="True">
				<graphicData>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>(1,4)</drawSize>
					<damageData>
						<cornerTL>Damage/Corner</cornerTL>
						<cornerTR>Damage/Corner</cornerTR>
						<cornerBL>Damage/Corner</cornerBL>
						<cornerBR>Damage/Corner</cornerBR>
					</damageData>
				</graphicData>
				<pathCost>60</pathCost>
				<statBases>
					<WorkToBuild>2800</WorkToBuild>
				</statBases>
				<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
			</ThingDef>
		<!-- MODULAR HYDROPONICS BASIN BASE -->
			<ThingDef ParentName="TMM_HydroponicsBase" Name="TMM_ModularHydroponicsBasinBase" Abstract="True">
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<linkType>Basic</linkType>
					<linkFlags>
						<li>Sandbags</li>
					</linkFlags>
					<damageData>
						<rect>(0.2,0.2,0.6,0.6)</rect>
					</damageData>
				</graphicData>
				<pathCost>40</pathCost>
				<statBases>
					<WorkToBuild>700</WorkToBuild>
				</statBases>
				<building>
					<blueprintGraphicData>
						<texPath>Things/Building/PlanterModular_Atlas</texPath>
					</blueprintGraphicData>
				</building>
				<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
			</ThingDef>
		<!-- ADVANCED HYDROPONICS BASIN BASE -->
			<ThingDef ParentName="TMM_HydroponicsBasinBase" Name="TMM_AdvancedHydroponicsBasinBase" Abstract="True">
				<researchPrerequisites>
					<li>TMM_HT_HydroponicsII</li>
				</researchPrerequisites>
			</ThingDef>
		<!-- ADVANCED MODULAR HYDROPONICS BASIN BASE -->
			<ThingDef ParentName="TMM_ModularHydroponicsBasinBase" Name="TMM_AdvancedModularHydroponicsBasinBase" Abstract="True">
				<researchPrerequisites>
					<li>TMM_HT_HydroponicsII</li>
				</researchPrerequisites>
			</ThingDef>
	<!-- PLANTER BOXES -->
		<!-- REGULAR -->
			<ThingDef ParentName="TMM_PlanterBoxBase">
				<defName>TMM_PlanterBox</defName>
				<label>Planter Box</label>
				<description>Crude box filled with soil, that allows you to grow anywhere. fertility is reduced to 70%. Does not require power.</description>
				<graphicData>
					<texPath>Modules/HarvestTime/Buildings/PlanterBoxes/PlanterBox</texPath>
					<drawSize>(4,1)</drawSize>
				</graphicData>
				<size>(4,1)</size>
				<costList>
					<TMM_DirtPile>12</TMM_DirtPile>
				</costList>
				<researchPrerequisites>
					<li>TMM_HT_PlanterBoxI</li>
				</researchPrerequisites>
			</ThingDef>
		<!-- MODULAR -->
			<ThingDef ParentName="TMM_ModularPlanterBase">
				<defName>TMM_ModularPlanterBox</defName>
				<label>Modular Planter Box</label>
				<description>Crude box filled with soil, that allows you to grow anywhere. fertility is reduced to 70%. Does not require power. Modular allows you to make 1x1 connected boxes of any size.</description>
				<thingClass>Building_PlantGrower</thingClass>
				<graphicData>
					<texPath>Modules/HarvestTime/Buildings/PlanterBoxes/PlanterModular_Atlas</texPath>
				</graphicData>
				<uiIconPath>Modules/HarvestTime/Icons/PlanterBoxes/PlanterIcon</uiIconPath>
				<costList>
					<TMM_DirtPile>4</TMM_DirtPile>
				</costList>
				<building>
					<blueprintGraphicData>
						<texPath>Modules/HarvestTime/Buildings/PlanterBoxes/PlanterModular_Atlas</texPath>
					</blueprintGraphicData>
				</building>
				<researchPrerequisites>
					<li>TMM_HT_PlanterBoxI</li>
				</researchPrerequisites>
			</ThingDef>
		<!-- ADVANCED MODULAR -->
			<ThingDef ParentName="TMM_ModularPlanterBase">
				<defName>TMM_AdvancedModularPlanterBox</defName>
				<label>Advanced Modular Planter Box</label>
				<description>Upgraded planter box filled with soil and fertilizer, that allows you to grow anywhere. Fertility is 120%. Does not require power. Modular allows you to make 1x1 connected boxes of any size. But is not movable.</description>
				<thingClass>Building_PlantGrower</thingClass>
				<graphicData>
					<texPath>Modules/HarvestTime/Buildings/PlanterBoxes/PlanterModular_Atlas</texPath>
				</graphicData>
				<uiIconPath>Modules/HarvestTime/Icons/PlanterBoxes/PlanterIcon</uiIconPath>
				<costList>
					<TMM_DirtPile>4</TMM_DirtPile>
					<TMM_RawFertiliser>4</TMM_RawFertiliser>
				</costList>
				<fertility>1.20</fertility>
				<building>
					<blueprintGraphicData>
						<texPath>Modules/HarvestTime/Buildings/PlanterBoxes/PlanterModular_Atlas</texPath>
					</blueprintGraphicData>
				</building>
				<researchPrerequisites>
					<li>TMM_HT_PlanterBoxII</li>
				</researchPrerequisites>
			</ThingDef>
		<!-- MODULAR FLOWER BOX -->
			<ThingDef ParentName="TMM_ModularPlanterBase">
				<defName>TMM_ModularFlowerBox</defName>
				<label>Modular Flower Box</label>
				<description>Flower box filled with soil. Modular allows you to make 1x1 connected boxes of any size.</description>
				<thingClass>Building_PlantGrower</thingClass>
				<graphicData>
					<texPath>Modules/HarvestTime/Buildings/PlanterBoxes/PlanterModular_Atlas</texPath>
				</graphicData>
				<uiIconPath>Modules/HarvestTime/Icons/PlanterBoxes/PlanterIcon</uiIconPath>
				<costStuffCount>8</costStuffCount>
				<fertility>1</fertility>
				<building>
					<blueprintGraphicData>
						<texPath>Modules/HarvestTime/Buildings/PlanterBoxes/PlanterModular_Atlas</texPath>
					</blueprintGraphicData>
					<defaultPlantToGrow>Plant_Rose</defaultPlantToGrow>
					<sowTag>Decorative</sowTag>
				</building>
				<researchPrerequisites>
					<li>TMM_FlowerFarming</li>
					<li>TMM_HT_PlanterBoxI</li>
				</researchPrerequisites>
			</ThingDef>

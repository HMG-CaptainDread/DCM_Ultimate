	<!-- TALLOW WAX -->
	<ThingDef ParentName="ResourceBase">
    <defName>Jecrell_Wax</defName>
    <label>tallow wax</label>
    <description>Wax created from animal fat.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Wax/Jecrell_Wax</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>400</stackLimit>
    <statBases>
      <Mass>0.1</Mass>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>3</MarketValue>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>2</DeteriorationRate>
      <WorkToMake>200</WorkToMake>
    </statBases>
    <stuffProps>
      <stuffAdjective>wax</stuffAdjective>
      <categories>
        <li>Stony</li>
      </categories>
      <commonality>1.00</commonality>
      <color>(239,230,211)</color>
      <constructEffect>ConstructWood</constructEffect>
      <soundImpactBullet>BulletImpact_Wood</soundImpactBullet>
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
      <statFactors>
        <MaxHitPoints>0.05</MaxHitPoints>
        <Beauty>1</Beauty>
        <Flammability>1</Flammability>
        <WorkToMake>1.0</WorkToMake>
        <WorkToBuild>1.0</WorkToBuild>
        <DoorOpenSpeed>1.0</DoorOpenSpeed>
        <MeleeWeapon_CooldownMultiplier>1.0</MeleeWeapon_CooldownMultiplier>
      </statFactors>
    </stuffProps>
    <intricate>true</intricate>
    <smeltable>true</smeltable>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>

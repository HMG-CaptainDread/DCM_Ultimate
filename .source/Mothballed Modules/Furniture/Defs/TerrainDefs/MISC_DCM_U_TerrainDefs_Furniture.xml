	<!-- ANON 2 FLOOR BASE -->
	<TerrainDef Name="Anon2FloorBase" Abstract="True">
    <layerable>true</layerable>
    <affordances>
      <li>Light</li>
      <li>Medium</li>
      <li>Heavy</li>
    </affordances>
    <statBases>
      <WorkToBuild>135</WorkToBuild>
    </statBases>
    <designationCategory>Floors</designationCategory>
    <fertility>0</fertility>
    <constructEffect>ConstructDirt</constructEffect>
    <filthAcceptanceMask>Unnatural</filthAcceptanceMask>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
  </TerrainDef>
	<!-- ANON 2 CARPET BASE -->
	<TerrainDef Name="Anon2CarpetBase" ParentName="Anon2FloorBase" Abstract="True">
    <texturePath>Terrain/Surfaces/Carpet</texturePath>
    <researchPrerequisites>
      <li>CarpetMaking</li>
    </researchPrerequisites>
    <burnedDef>BurnedCarpet</burnedDef>
    <costList>
      <Cloth>7</Cloth>
    </costList>
    <tags>
      <li>Floor</li>
    </tags>
    <statBases>
      <Beauty>2</Beauty>
      <Flammability>0.32</Flammability>
      <CleaningTimeFactor>2</CleaningTimeFactor>
    </statBases>
  </TerrainDef>

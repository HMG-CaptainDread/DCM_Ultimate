		<!-- SMASH LIMESTONE ROCKS -->
			<RecipeDef ParentName="SmashStoneBlocksBase">
				<defName>VBY_SmashRocks</defName>
				<label>smash limestone rocks</label>
				<description>Smash two limestone rocks together to break into smaller fragments of flint.</description>
				<ingredients>
					<li>
						<filter>
							<thingDefs>
								<li>ChunkLimestone</li>
							</thingDefs>
						</filter>
						<count>2</count>
					</li>
				</ingredients>
				<fixedIngredientFilter>
					<thingDefs>
						<li>ChunkLimestone</li>
					</thingDefs>
				</fixedIngredientFilter>
				<products>
					<VBY_PrimitiveFlint>20</VBY_PrimitiveFlint>
				</products>
				<workSkill>Crafting</workSkill>
			</RecipeDef>

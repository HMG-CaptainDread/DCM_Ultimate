	<!-- VANILLA CHEMFUEL EXPANDED UNOFFICIAL -->
		<!-- MAKE DYE FROM WASTE -->
			<PipeSystem.ProcessDef>
				<defName>VCEUE_WasteIntoDye</defName>
				<label>Make Dye from Waste</label>
				<ingredients>
					<li>
						<pipeNet>VCEUE_ToxicWasteNet</pipeNet>
						<thing>Wastepack</thing>
						<countNeeded>7</countNeeded>
					</li>
					<li>
						<pipeNet>VREA_NeutroamineNet</pipeNet>
						<thing>Neutroamine</thing>
						<countNeeded>3</countNeeded>
					</li>
				</ingredients>
				<ticks>45000</ticks>
				<results>
					<li>
						<pipeNet>VCEUE_DyeNet</pipeNet>
						<thing>Dye</thing>
						<count>35</count>
					</li>
				</results>
				<destroyIngredientsDirectly>true</destroyIngredientsDirectly>
				<researchPrerequisites>
					<li>VCEUE_Biotech_DyeRefining</li>
				</researchPrerequisites>
			</PipeSystem.ProcessDef>

            <ThingDef ParentName="DCM_MineableBase">
                <defName>DCM_U_MineableBauxite</defName>
                <label>bauxite</label>
                <description>A sedimentary rock that, when processed will eventually produce aluminium.</description>
                <graphicData>
                    <color>(254,254,254)</color>
                    <colorTwo>(128,128,128)</colorTwo>
                </graphicData>
                <statBases>
                    <MaxHitPoints>900</MaxHitPoints>
                </statBases>
                <building>
                    <mineableThing>DCM_U_Bauxite</mineableThing>
                    <mineableYield>50</mineableYield>
                    <mineableScatterCommonality>1.1</mineableScatterCommonality>
                    <mineableScatterLumpSizeRange>12~25</mineableScatterLumpSizeRange>
                </building>
            </ThingDef>

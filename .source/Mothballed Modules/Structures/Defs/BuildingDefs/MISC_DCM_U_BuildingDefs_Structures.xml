	<!-- DOUBLE DOORS -->
		<!-- DOUBLE DOOR -->
			<ThingDef ParentName="DoorBase">
				<defName>BDD_DoubleDoor</defName>
				<label>double door</label>
				<description>Divides rooms. Simple double doors must be manually opened, which slows people down. The amount of slowdown depends on what the door is made of.</description>
				<thingClass>Building_MultiTileDoor</thingClass>
				<statBases>
					<WorkToBuild>1700</WorkToBuild>
					<MaxHitPoints>200</MaxHitPoints>
				</statBases>
				<graphicData>
					<texPath>Doors/BDD_DoorSimple_DoubleMover</texPath>
					<graphicClass>Graphic_Single</graphicClass>
					<damageData>
						<enabled>false</enabled>
					</damageData>
				</graphicData>
				<uiIconPath>Doors/BDD_DoorSimple_DoubleMenuIcon</uiIconPath>
				<useBlueprintGraphicAsGhost>true</useBlueprintGraphicAsGhost>
				<size>(2, 1)</size>
				<rotatable>true</rotatable>
				<costStuffCount>50</costStuffCount>
				<building>
					<isStuffableAirtight>true</isStuffableAirtight>
					<blueprintClass>Blueprint_Build</blueprintClass>
					<blueprintGraphicData>
						<texPath>Doors/BDD_DoorSimple_DoubleMenuIcon</texPath>
						<graphicClass>Graphic_Multi</graphicClass>
						<shaderType>EdgeDetect</shaderType>
						<drawSize>(2, 2)</drawSize>
					</blueprintGraphicData>
				</building>
				<placeWorkers>
					<li>PlaceWorker_MultiCellDoor</li>
				</placeWorkers>
			</ThingDef>
		<!-- DOUBLE AUTODOOR -->
			<ThingDef ParentName="DoorBase">
				<defName>BDD_AutoDoor</defName>
				<label>double autodoor</label>
				<description>Divides rooms. Powered operation allows people to move through the door without slowing down.</description>
				<thingClass>Building_MultiTileDoor</thingClass>
				<statBases>
					<WorkToBuild>2200</WorkToBuild>
					<MaxHitPoints>200</MaxHitPoints>
				</statBases>
				<graphicData>
					<texPath>Doors/BDD_Autodoor_DoubleMover</texPath>
					<graphicClass>Graphic_Single</graphicClass>
					<damageData>
						<enabled>false</enabled>
					</damageData>
				</graphicData>
				<uiIconPath>Doors/BDD_Autodoor_DoubleMenuIcon</uiIconPath>
				<useBlueprintGraphicAsGhost>true</useBlueprintGraphicAsGhost>
				<size>(2, 1)</size>
				<rotatable>true</rotatable>
				<costList>
					<Steel>80</Steel>
					<ComponentIndustrial>3</ComponentIndustrial>
				</costList>
				<costStuffCount>50</costStuffCount>
				<building>
					<isStuffableAirtight>true</isStuffableAirtight>
					<blueprintClass>Blueprint_Build</blueprintClass>
					<blueprintGraphicData>
						<texPath>Doors/BDD_Autodoor_DoubleMenuIcon</texPath>
						<graphicClass>Graphic_Multi</graphicClass>
						<shaderType>EdgeDetect</shaderType>
						<drawSize>(2, 2)</drawSize>
					</blueprintGraphicData>
				</building>
				<comps>
					<li Class="CompProperties_Power">
						<compClass>CompPowerTrader</compClass>
						<basePowerConsumption>50</basePowerConsumption>
					</li>
					<li Class="CompProperties_Breakdownable"/>
					<li Class="CompProperties_Styleable" />
				</comps>
				<placeWorkers>
					<li>PlaceWorker_MultiCellDoor</li>
				</placeWorkers>
				<researchPrerequisites>
					<li>Autodoors</li>
				</researchPrerequisites>
				<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
			</ThingDef>
	<!-- DOORS EXPANDED -->
		<!-- CLOTH DOORS BASE -->
			<ThingDef ParentName="AbstractHeronDoorBase" Name="HeronClothDoorBase" Abstract="True">

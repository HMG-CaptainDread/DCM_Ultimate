	<!-- VCE - STEWS -->
		<!-- SIMPLE STEW -->
			<PipeSystem.ProcessDef>
				<defName>VCE_SimpleStewRecipe</defName>
				<label>Cook Simple Stew</label>
				<description>Turns an uncooked simple stew into 10 cooked simple stews.</description>
				<spawnOnInteractionCell>true</spawnOnInteractionCell>
				<autoExtract>false</autoExtract>
				<priorityInBillList>0</priorityInBillList>
				<ingredients>
					<li>
						<thing>VCE_UncookedStewSimple</thing>
						<countNeeded>1</countNeeded>
					</li>
				</ingredients>
				<ticks>180000</ticks>
				<results>
					<li>
						<thing>VCE_CookedStewSimple</thing>
						<count>10</count>
					</li>
				</results>
				<noPowerDestroysProgress>true</noPowerDestroysProgress>
				<noPowerDestroysMessage>VCE_StewFailurePower</noPowerDestroysMessage>
				<noPowerDestroysInitialWarning>VCE_StewFailurePowerWarning</noPowerDestroysInitialWarning>
				<rareTicksToDestroy>10</rareTicksToDestroy>
				<destroyIngredientsOnStart>true</destroyIngredientsOnStart>
			</PipeSystem.ProcessDef>

	<!-- MAKE PATCHLEATHER -->
		<RecipeDef>
			<defName>Make_PatchleatherBulk</defName>
			<label>make patchleather x4</label>
			<description>Combine various types of leather to create weak patchleather. \n\nMakes 200 Patch Leathers.</description>
			<jobString>Making patchleather.</jobString>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<effectWorking>Tailor</effectWorking>
			<soundWorking>Recipe_Tailor</soundWorking>
			<workAmount>3500</workAmount>
			<allowMixingIngredients>true</allowMixingIngredients>
			<targetCountAdjustment>200</targetCountAdjustment>
			<recipeUsers>
			<li>ElectricTailoringBench</li>
			<li>HandTailoringBench</li>
			<li>TableLoom</li>
			<li>TableElecLoom</li>
			</recipeUsers>
			<ingredients>
			<li>
				<filter>
				<categories>
					<li>Leathers</li>
				</categories>
				<disallowedThingDefs>
					<li>Leather_Thrumbo</li>
				</disallowedThingDefs>
				</filter>
				<count>200</count>
			</li>
			</ingredients>
			<fixedIngredientFilter>
			<categories>
				<li>Leathers</li>
			</categories>
			<disallowedThingDefs>
				<li>Leather_Patch</li>
				<li>Leather_Human</li>
			</disallowedThingDefs>
			</fixedIngredientFilter>
			<products>
			<Leather_Patch>200</Leather_Patch>
			</products>
			<workSkill>Crafting</workSkill>
		</RecipeDef>

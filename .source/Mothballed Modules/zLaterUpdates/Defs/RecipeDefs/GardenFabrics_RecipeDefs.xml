	<!-- WEAVE LINEN CLOTHS -->
		<RecipeDef>
				<defName>MakeLinenHand</defName>
				<label>weave five linen cloths</label>
				<description>weave linen cloth from raw flax. \n\nMakes 5 cloth.</description>
				<jobString>weaving linen cloth.</jobString>
				<workAmount>1000</workAmount>
				<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
				<effectWorking>Tailor</effectWorking>
				<soundWorking>Recipe_Tailor</soundWorking>
				<ingredients>
					<li>
						<filter>
							<thingDefs>
								<li>RawFlax</li>
							</thingDefs>
						</filter>
						<count>5</count>
					</li>
				</ingredients>
				<fixedIngredientFilter>
					<thingDefs>
						<li>RawFlax</li>
					</thingDefs>
				</fixedIngredientFilter>
				<products>
					<Flaxcloth>5</Flaxcloth>
				</products>
				<workSkill>Crafting</workSkill>
			<recipeUsers>
				<li>CraftingSpot</li>
			</recipeUsers>
		</RecipeDef>

	<!-- MAKE SILAGE -->
		<RecipeDef>
			<defName>MakeSilage</defName>
			<label>make silage</label>
			<description>Make silage from hay and raw grains. \n\nMakes 50.</description>
			<jobString>Making silage.</jobString>
			<workSpeedStat>CookSpeed</workSpeedStat>
			<effectWorking>Cook</effectWorking>
			<soundWorking>Recipe_CookMeal</soundWorking>
			<allowMixingIngredients>true</allowMixingIngredients>
			<workAmount>900</workAmount>
			<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>		
			<ingredients>
				<li>
					<filter>
						<thingDefs>
						<li>RawCorn</li>
						<li>RawRice</li>
						<li>RawBarley</li>
						<li>Rawbean</li>
						</thingDefs>
					</filter>
					<count>1</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Hay</li>
						</thingDefs>
					</filter>
					<count>1</count>
				</li>
			</ingredients>
			<fixedIngredientFilter>
				<thingDefs>
					<li>RawCorn</li>
					<li>RawRice</li>
					<li>RawBarley</li>
					<li>Rawbean</li>
					<li>Hay</li>
				</thingDefs>
			</fixedIngredientFilter>
			<products>
				<Silage>50</Silage>
			</products>
			<workSkill>Cooking</workSkill>
			<recipeUsers>
				<li>TableButcher</li>
			</recipeUsers>
		</RecipeDef>

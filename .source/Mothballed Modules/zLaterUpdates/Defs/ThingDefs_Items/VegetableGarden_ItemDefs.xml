	<!-- FRUIT JUICE DRINK -->
		<ThingDef ParentName="VG_MealsBase">
			<defName>Fruitdrink</defName>
			<label>fruit juice drink</label>
			<description>A drink made from pressing fruit for their juice. \n\nDrink Effects: Consciousness and Blood Filtration.</description>

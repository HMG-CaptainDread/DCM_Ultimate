			</statBases>
			<ingestible>
				<preferability>NeverForNutrition</preferability>
				<foodType>Fluid, Processed, Liquor</foodType>
				<joy>0.25</joy>
				<joyKind>Gluttonous</joyKind>
				<nurseable>true</nurseable>

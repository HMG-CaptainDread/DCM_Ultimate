	<!-- DANDELION TEA HIGH -->
		<HediffDef>
			<defName>dandelionTeaHigh</defName>
			<label>high on dandelion tea</label>
			<labelNoun>a dandelion tea high</labelNoun>
			<description>Active dandelion tea in the bloodstream. Generates a mild boosted immunity effect. Allowing faster recoveries to diseases and injures.</description>
			<hediffClass>Hediff_High</hediffClass>
			<defaultLabelColor>(1,0,0.5)</defaultLabelColor>
			<scenarioCanAdd>true</scenarioCanAdd>
			<maxSeverity>1.0</maxSeverity>
			<isBad>false</isBad>
			<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-1.0</severityPerDay>
				<showHoursToRecover>true</showHoursToRecover>
			</li>
			</comps>
			<stages>
				<li>
				<minSeverity>1.0</minSeverity>
				<label>boosted immunity</label>
					<capMods>
					<li>
						<capacity>BloodFiltration</capacity>
						<offset>0.15</offset>
					</li>
					</capMods>
				</li>
			</stages>
		</HediffDef>
	<!-- ANTIBIOTICS -->
		<HediffDef>
			<defName>Antibiotics</defName>
			<label>antibiotics</label>
			<description>Antibiotics are used to speed up the recovery time from sickness. Helps prevent infections.</description>
			<defaultLabelColor>(0.062, 0.76, 0.05)</defaultLabelColor>
			<initialSeverity>0.35</initialSeverity>
			<hediffClass>HediffWithComps</hediffClass>
			<isBad>false</isBad>
			<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-0.25</severityPerDay>
			</li>
			</comps>
			<stages>
					<li>
				<minSeverity>0.20</minSeverity>
				<makeImmuneTo>
					<li>WoundInfection</li>
				</makeImmuneTo>
				</li>
				<li>
				<minSeverity>0.25</minSeverity>
				<label>boosted immunedefency</label>
					<capMods>
					<li>

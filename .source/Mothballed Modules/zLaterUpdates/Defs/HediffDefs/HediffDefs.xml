	<!-- CAFFEINE -->
		<HediffDef>
			<defName>Caffeine</defName>
			<label>caffeine</label>
			<description>Caffeine is flowing through your viens! You need less sleep. And move faster.</description>
			<initialSeverity>2</initialSeverity>
			<defaultLabelColor>(0.062, 0.76, 0.05)</defaultLabelColor>
			<hediffClass>HediffWithComps</hediffClass>
			<isBad>false</isBad>
			<comps>
				<li Class="HediffCompProperties_SeverityPerDay">
					<severityPerDay>-1.0</severityPerDay>
				</li>
			</comps>
			<stages>
				<li>
				<minSeverity>0.01</minSeverity>
				<label>wired</label>
					<capMods>
						<li>
							<capacity>Moving</capacity>
							<offset>0.1</offset>
						</li>
						<li>
							<capacity>Consciousness</capacity>
							<offset>0.1</offset>
						</li>
					</capMods>
				</li>
				<li>
				<minSeverity>0.15</minSeverity>
				<label>jacked up</label>
					<capMods>
						<li>
							<capacity>Moving</capacity>
							<offset>0.2</offset>
						</li>
						<li>
							<capacity>Consciousness</capacity>
							<offset>0.15</offset>
						</li>
					</capMods>
				</li>
				<li>
				<minSeverity>0.35</minSeverity>
				<label>blackout</label>
					<capMods>
						<li>
							<capacity>Consciousness</capacity>
							<setMax>0.1</setMax>
						</li>
					</capMods>
				</li>
			</stages>
		</HediffDef>

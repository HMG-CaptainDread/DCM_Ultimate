<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- <></> -->
	<!--  -->



	<!-- RIMMU-NATION -->
		<!-- BOMB -->
			<DamageDef Name="RNBomb">
				<defName>RN2Bomb</defName>
				<label>bomb</label>
				<workerClass>DamageWorker_AddInjury</workerClass>
				<externalViolence>true</externalViolence>
				<isExplosive>true</isExplosive>
				<deathMessage>{0} has died in an explosion.</deathMessage>
				<hediff>Shredded</hediff>
				<hediffSolid>Crack</hediffSolid>
				<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
				<impactSoundType>Blunt</impactSoundType>
				<armorCategory>Sharp</armorCategory>
				<minDamageToFragment>15</minDamageToFragment>
				<defaultDamage>50</defaultDamage>
				<defaultStoppingPower>1</defaultStoppingPower>
				<defaultArmorPenetration>0.25</defaultArmorPenetration>
				<buildingDamageFactor>4</buildingDamageFactor>
				<plantDamageFactor>2</plantDamageFactor>
				<explosionAffectOutsidePartsOnly>false</explosionAffectOutsidePartsOnly>
				<explosionHeatEnergyPerCell>5</explosionHeatEnergyPerCell>
				<explosionCellFleck>BlastDry</explosionCellFleck>
				<explosionColorCenter>(255, 147, 31)</explosionColorCenter>
				<explosionColorEdge>(105, 105, 105)</explosionColorEdge>
				<soundExplosion>RNExplosion</soundExplosion>
				<combatLogRules>Damage_Bomb</combatLogRules>
			</DamageDef>
		<!-- FRAG BOMB -->
			<DamageDef Name="RN2FragBomb">
				<defName>RN2FragBomb</defName>
				<label>bomb</label>
				<workerClass>DamageWorker_AddInjury</workerClass>
				<externalViolence>true</externalViolence>
				<isExplosive>true</isExplosive>
				<deathMessage>{0} has died in an explosion.</deathMessage>
				<hediff>Shredded</hediff>
				<hediffSolid>Crack</hediffSolid>
				<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
				<impactSoundType>Blunt</impactSoundType>
				<armorCategory>Sharp</armorCategory>
				<minDamageToFragment>12</minDamageToFragment>
				<defaultDamage>32</defaultDamage>
				<defaultStoppingPower>2</defaultStoppingPower>
				<defaultArmorPenetration>0.25</defaultArmorPenetration>
				<buildingDamageFactor>4</buildingDamageFactor>
				<plantDamageFactor>2</plantDamageFactor>
				<explosionAffectOutsidePartsOnly>false</explosionAffectOutsidePartsOnly>
				<explosionHeatEnergyPerCell>5</explosionHeatEnergyPerCell>
				<explosionCellFleck>BlastDry</explosionCellFleck>
				<explosionColorCenter>(255, 147, 31)</explosionColorCenter>
				<explosionColorEdge>(105, 105, 105)</explosionColorEdge>
				<soundExplosion>RN2FragExplosion</soundExplosion>
				<combatLogRules>Damage_Bomb</combatLogRules>
			</DamageDef>
		<!-- CHARGE -->
			<DamageDef>
				<defName>RNBullet_Charge</defName>
				<label>charge</label>
				<workerClass>DamageWorker_AddInjury</workerClass>
				<externalViolence>true</externalViolence>
				<deathMessage>{0} has been shot to death.</deathMessage>
				<hediff>Shredded</hediff>
				<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
				<impactSoundType>Bullet</impactSoundType>
				<!--<armorCategory>Heat</armorCategory>-->
				<armorCategory>Sharp</armorCategory>
				<overkillPctToDestroyPart>0~0.7</overkillPctToDestroyPart>
				<isRanged>true</isRanged>
				<makesAnimalsFlee>true</makesAnimalsFlee>
			</DamageDef>
	<!-- AA BOMB -->
		<DamageDef>
			<defName>CGF1_MWP_DamageDef_Bomb_MANPADS</defName>
			<label>AA bomb</label>
			<workerClass>DamageWorker_AddInjury</workerClass>
			<externalViolence>true</externalViolence>
			<isExplosive>true</isExplosive>
			<deathMessage>{0} has died in an explosion.</deathMessage>
			<hediff>Shredded</hediff>
			<hediffSolid>Crack</hediffSolid>
			<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
			<impactSoundType>Blunt</impactSoundType>
			<armorCategory>Sharp</armorCategory>
			<minDamageToFragment>15</minDamageToFragment>
			<defaultDamage>50</defaultDamage>
			<defaultStoppingPower>0.5</defaultStoppingPower>
			<defaultArmorPenetration>0.10</defaultArmorPenetration>
			<buildingDamageFactorImpassable>100</buildingDamageFactorImpassable>
			<buildingDamageFactorPassable>100</buildingDamageFactorPassable>
			<plantDamageFactor>0.5</plantDamageFactor>
			<corpseDamageFactor>0.1</corpseDamageFactor>
			<explosionAffectOutsidePartsOnly>false</explosionAffectOutsidePartsOnly>
			<explosionHeatEnergyPerCell>5</explosionHeatEnergyPerCell>
			<explosionCellFleck>BlastDry</explosionCellFleck>
			<explosionColorCenter>(1, 0.5, 0.3)</explosionColorCenter>
			<explosionColorEdge>(0.6, 0.5, 0.4)</explosionColorEdge>
			<soundExplosion>Explosion_Bomb</soundExplosion>
			<combatLogRules>Damage_Bomb</combatLogRules>
		</DamageDef>
	<!-- AT BOMB -->
		<DamageDef>
			<defName>CGF1_MWP_DamageDef_Bomb_ATMissile</defName>
			<label>AT bomb</label>
			<workerClass>DamageWorker_AddInjury</workerClass>
			<externalViolence>true</externalViolence>
			<isExplosive>true</isExplosive>
			<deathMessage>{0} has died in an explosion.</deathMessage>
			<hediff>Shredded</hediff>
			<hediffSolid>Crack</hediffSolid>
			<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
			<impactSoundType>Blunt</impactSoundType>
			<armorCategory>Sharp</armorCategory>
			<minDamageToFragment>5</minDamageToFragment>
			<defaultDamage>800</defaultDamage>
			<defaultStoppingPower>0.5</defaultStoppingPower>
			<defaultArmorPenetration>100</defaultArmorPenetration>
			<buildingDamageFactorImpassable>4</buildingDamageFactorImpassable>
			<buildingDamageFactorPassable>2</buildingDamageFactorPassable>
			<plantDamageFactor>4</plantDamageFactor>
			<explosionAffectOutsidePartsOnly>false</explosionAffectOutsidePartsOnly>
			<explosionHeatEnergyPerCell>5</explosionHeatEnergyPerCell>
			<explosionCellFleck>BlastDry</explosionCellFleck>
			<explosionColorCenter>(1, 0.5, 0.3)</explosionColorCenter>
			<explosionColorEdge>(0.6, 0.5, 0.4)</explosionColorEdge>
			<soundExplosion>Explosion_Bomb</soundExplosion>
			<combatLogRules>Damage_Bomb</combatLogRules>
		</DamageDef>
	<!-- BLUNT PROJECTILE -->
		<DamageDef>
			<defName>CGF1_MWP_DamageDef_BluntProjectile</defName>
			<label>projectile</label>
			<workerClass>DamageWorker_AddInjury</workerClass>
			<externalViolence>true</externalViolence>
			<deathMessage>{0} has died of blunt trauma.</deathMessage>
			<hediff>Crush</hediff>
			<hediffSkin>Bruise</hediffSkin>
			<hediffSolid>Crack</hediffSolid>
			<harmAllLayersUntilOutside>true</harmAllLayersUntilOutside>
			<impactSoundType>Bullet</impactSoundType>
			<armorCategory>Sharp</armorCategory>
			<overkillPctToDestroyPart>0.5~1</overkillPctToDestroyPart>
			<isRanged>true</isRanged>
			<makesAnimalsFlee>true</makesAnimalsFlee>
		</DamageDef>
	<!-- CS GAS -->
		<DamageDef MayRequire="VanillaExpanded.VWENL">
			<defName>CGF1_MWP_Damage_CSGas</defName> <!-- independent -->
			<label>CS gas</label>
			<workerClass>VanillaWeaponsExpandedNonLethal.DamageWorker_TearGas</workerClass>
			<canInterruptJobs>false</canInterruptJobs>
			<makesBlood>false</makesBlood>
			<defaultDamage>0</defaultDamage>
			<explosionCellFleck>BlastExtinguisher</explosionCellFleck>
			<explosionColorEdge>(134,144,22,0.05)</explosionColorEdge>
			<soundExplosion>Explosion_Stun</soundExplosion>
			<harmsHealth>false</harmsHealth>
			<combatLogRules>Damage_Extinguish</combatLogRules>
		</DamageDef>

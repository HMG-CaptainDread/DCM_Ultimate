	<!-- 6 - ARCHOTECH -->
		<!-- ARCHOTECH NANOSKIN -->
			<ThingDef ParentName="BodyPartArchotechBase">
				<defName>ArchotechNanoskin</defName>
				<label>archotech nanoskin</label>
				<description>A genetically modified skin built by an archotech. The remarkable self-replicating archotech naturally replaces the skin with an advanced pseudoskin that is nearly imperceptibly different to the naked eye but which is much more resistent to damage while also subtly aesthetically pleasing. It is far beyond human means to understand let alone manufacture.</description>
				<descriptionHyperlinks>
					<RecipeDef>InstallArchotechNanoskin</RecipeDef>
				</descriptionHyperlinks>
				<statBases>
					<MarketValue>2800</MarketValue>
					<Mass>0.2</Mass>
				</statBases>
				<thingSetMakerTags>
					<li>RewardStandardLowFreq</li>
				</thingSetMakerTags>
			</ThingDef>
		<!-- CEREBRAL TURBOCHARGER -->
			<ThingDef ParentName="BodyPartArchotechBase">
				<defName>ArchotechCerebralTurbocharger</defName>
				<label>cerebral turbocharger</label>
				<description>A brain implant built by an archotech. Beyond any ability for human minds to comprehend, even after installation, the archotech cerebral turbocharger induces a mental state of perfect acuity making nearly any task easier.</description>
				<descriptionHyperlinks>
					<RecipeDef>InstallArchotechCerebralTurbocharger</RecipeDef>
				</descriptionHyperlinks>
				<statBases>
					<MarketValue>3200</MarketValue>
					<Mass>0.1</Mass>
				</statBases>
				<thingSetMakerTags>
					<li>RewardStandardLowFreq</li>
				</thingSetMakerTags>
			</ThingDef>
		<!-- NEURAL RECHARGER -->
			<ThingDef ParentName="BodyPartArchotechBase">
				<defName>ArchotechNeuralRecharger</defName>

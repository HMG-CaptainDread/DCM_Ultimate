<Defs>
  <RecipeDef>
    <defName>VCEUE_Make_Plasteel_2</defName>
    <label>make plasteel from raw plasteel x2</label>
    <description>Refine raw plasteel into plasteel using advanced techniques. This recipe outputs twice as much plasteel, but requires twice the amount of raw plasteel and takes longer to complete.</description>
    <jobString>Refining plasteel from raw plasteel x2.</jobString>
    <effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <workAmount>2000</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>VCEUE_RawPlasteel</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>VCEUE_RawPlasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Plasteel>35</Plasteel>
    </products>
    <recipeUsers>
      <li>ElectricSmelter</li>
      <li MayRequire="vanillaexpanded.vfeproduction">VFE_FueledSmelter</li>
      <li MayRequire="vanillaexpanded.helixiengas">VHGE_GasPoweredSmelter</li>
    </recipeUsers>
  </RecipeDef>


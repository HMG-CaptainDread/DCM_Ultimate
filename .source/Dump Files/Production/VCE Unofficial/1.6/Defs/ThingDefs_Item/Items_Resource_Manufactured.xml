<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <ThingDef ParentName="ResourceBase">
    <defName>VCEUE_RawPlasteel</defName>
    <label>raw plasteel</label>
    <description>Unrefined chunks of plasteel. Can be smelted into usable plasteel bars.</description>
    <graphicData>
      <texPath>Things/Item/Resource/VCEUE_RawPlasteel</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <statBases>
      <MaxHitPoints>25</MaxHitPoints>
      <MarketValue>5</MarketValue>
      <Mass>1</Mass>
      <Flammability>0.6</Flammability>
      <DeteriorationRate>5.0</DeteriorationRate>
    </statBases>
    <intricate>false</intricate>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
  </ThingDef>

  <ThingDef ParentName="ResourceBase">
    <defName>VCEUE_RawThread</defName>
    <label>Unrefined fibers</label>
    <description>Unrefined fibers refined from deepchem. They can be weaved into synthread, hyperweave or devilstrand with proper techniques. Due to their plasticity, they are very fragile.</description>
    <graphicData>
      <texPath>Things/Item/Resource/VCEUE_RawThread</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>10</MarketValue>
      <Mass>0.5</Mass>
      <Flammability>2</Flammability>
      <DeteriorationRate>3.5</DeteriorationRate>
    </statBases>
    <intricate>false</intricate>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
  </ThingDef>

<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef ParentName="BaseFilth">
        <defName>Filth_Neutroamine</defName>
        <label>neutroamine puddle</label>
        <statBases>
            <Beauty>-12</Beauty>
            <Cleanliness>-3</Cleanliness>
        </statBases>
        <graphicData>
            <texPath>Things/Filth/PoolSoft</texPath>
            <color>(122, 140, 170, 180)</color>
            <drawSize>2</drawSize>
        </graphicData>
        <filth>
            <disappearsInDays>0.2~0.4</disappearsInDays>
            <cleaningWorkToReduceThickness>30</cleaningWorkToReduceThickness>
            <canFilthAttach>true</canFilthAttach>
            <allowsFire>false</allowsFire>
            <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
        </filth>
    </ThingDef>

    <ThingDef ParentName="BaseFilth">
        <defName>Filth_Slime</defName>
        <label>slime</label>
        <statBases>
            <Beauty>-25</Beauty>
            <Cleanliness>-5</Cleanliness>
        </statBases>
        <graphicData>
            <texPath>Things/Filth/Spatter</texPath>
            <color>(130, 142, 120, 170)</color>
        </graphicData>
        <filth>
            <disappearsInDays>35~40</disappearsInDays>
            <rainWashes>true</rainWashes>
            <cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
            <canFilthAttach>true</canFilthAttach>
            <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
        </filth>
    </ThingDef>
</Defs>
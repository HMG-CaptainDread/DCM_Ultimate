<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <!-- Chemfuel Network -->
    <PipeSystem.ProcessDef>
        <defName>VCEUE_DeepchemIntoChemfuelTierII</defName>
        <label>Make Chemfuel from Deepchem faster.</label>
        <ingredients>
            <li>
                <pipeNet>VCHE_DeepchemNet</pipeNet>
                <thing>VCHE_Deepchem</thing>
                <countNeeded>3</countNeeded>
            </li>
        </ingredients>
        <ticks>200</ticks>
        <results>
            <li>
                <pipeNet>VCHE_ChemfuelNet</pipeNet>
                <thing>Chemfuel</thing>
                <count>9</count>
            </li>
        </results>
        <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
        <researchPrerequisites>
            <li>VCEUE_ChemfuelRefiningTierII</li>
        </researchPrerequisites>
    </PipeSystem.ProcessDef>


  <ThingDef ParentName="BuildingBase">
    <defName>VCEUE_NeutroamineTank</defName>
    <label>Neutroamine tank</label>
    <description>A sterile industrial liquid storage tank meant for storing Neutroamine delivered via Neutroamine pipe network. Tanks can be designated to be siphoned for usable Neutroamine as needed.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Building/Production/NeutroamineTank/NeutroamineTank</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(4,4)</drawSize>
      <drawOffsetEast>(0,2,-0.2)</drawOffsetEast>
      <drawOffsetWest>(0,2,-0.2)</drawOffsetWest>
      <drawOffsetNorth>(0,2,0.3)</drawOffsetNorth>
      <drawOffsetSouth>(0,2,0.3)</drawOffsetSouth>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>1200</WorkToBuild>
      <Mass>20</Mass>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(2,3)</size>
    <costList>
      <Silver>300</Silver>
    </costList>
    <comps>
      <li Class="PipeSystem.CompProperties_ResourceStorage">
        <pipeNet>VREA_NeutroamineNet</pipeNet>
        <storageCapacity>1000</storageCapacity>
        <barSize>(1.3, 0.25)</barSize>
        <margin>0.1</margin>
        <centerOffset>(0, 0, 0.35)</centerOffset>
        <extractOptions>
          <texPath>UI/Gizmos/ExtractNeutroamine</texPath>
          <labelKey>VCEUE_ExtractNeutroamine</labelKey>
          <descKey>VCEUE_ExtractNeutroamineDesc</descKey>
          <extractAmount>150</extractAmount>
          <thing>Neutroamine</thing>
          <ratio>1</ratio>
        </extractOptions>
      </li>
    </comps>
    <building>
      <relatedBuildCommands>
        <li>VCEUE_NeutroamineRefinery</li>
        <li>VREA_NeutroPipe</li>
        <li>VREA_SubterraneanNeutroPipe</li>
        <li>VCEUE_NeutroamineValve</li>
        <li>VCEUE_NeutroamineTank</li>
        <li>VCEUE_NeutroamineTap</li>
        <li>VCEUE_NeutroamineRefinery</li>
        <li>VCEUE_NeutroamineDrain</li>
      </relatedBuildCommands>
    </building>

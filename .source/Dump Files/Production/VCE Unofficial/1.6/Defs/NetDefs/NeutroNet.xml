  <ThingDef ParentName="BuildingBase">
    <defName>VCEUE_NeutroamineRefinery</defName>
    <label>neutroamine refinery</label>
    <inspectorTabs>
      <li>PipeSystem.ITab_Processor</li>
    </inspectorTabs>
    <description>A complex machine capable of refining neutroamine from deepchem delivered via deepchem pipes into usable chemical components. If no output is detected, the chemical components will automatically be dispensed into cans. If the output is connected to a tank or tap via chemical pipes, the finished product will be transported as such for storage or dispensing.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Building/Production/neutroRefinery/neutroRefinery</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,4)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>2500</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(3,3)</size>
    <costList>
      <Silver>200</Silver>
      <ComponentIndustrial>4</ComponentIndustrial>
    </costList>
    <comps>
      <!-- <li MayRequire="ludeon.rimworld.biotech" Class="CompProperties_ThingContainer">
        <compClass>CompThingContainer</compClass>
        <stackLimit>100</stackLimit>
        <drawContainedThing>false</drawContainedThing>
        <dropEffecterDef>MechChargerWasteRemoved</dropEffecterDef>
      </li>
      <li MayRequire="ludeon.rimworld.biotech" Class="CompProperties_WasteProducer">
        <showContentsInInspectPane>true</showContentsInInspectPane>
      </li> -->
      <li Class="PipeSystem.CompProperties_AdvancedResourceProcessor">
        <showBufferInfo>false</showBufferInfo>
        <notWorkingKey>VCEUE_CantRefineFSX</notWorkingKey>
        <progressBarOffset>(0, 0, -0.65)</progressBarOffset>
        <showResultItem>true</showResultItem>
        <processes>
          <li>VCEUE_DeepchemIntoNeutroamine</li>
          <li>VCEUE_DeepchemIntoNeutroamineTierII</li>
          <li>VCEUE_DeepchemIntoNeutroamineTierIII</li>
        </processes>
        <!-- <soundAmbient>VCHE_RefineryAmbient</soundAmbient> -->
      </li>
      <li Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VREA_NeutroamineNet</pipeNet>
      </li>
      <li Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VCHE_DeepchemNet</pipeNet>
      </li>
      <!-- <li MayRequire="ludeon.rimworld.biotech" Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VCEUE_ToxicWasteNet</pipeNet>
      </li> -->
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>3000</basePowerConsumption>
        <shortCircuitInRain>false</shortCircuitInRain>
      </li>
    </comps>
    <building>
      <relatedBuildCommands>
        <li>PS_DeepchemRefinery</li>
        <li>VCHE_DeepchemPipe</li>
        <li>VCHE_UndergroundDeepchemPipe</li>
        <li>VCHE_DeepchemValve</li>
        <li>PS_DeepchemTank</li>
        <li>VCHE_DeepchemTap</li>
        <li>PS_DeepchemRefinery</li>
        <li>VCHE_DeepchemDrain</li>
        <li>VCHE_DeepchemPumpjack</li>
        <li>VCEUE_NeutroamineRefinery</li>
        <li>VREA_NeutroPipe</li>
        <li>VREA_SubterraneanNeutroPipe</li>
        <li>VCEUE_NeutroamineValve</li>
        <li>VCEUE_NeutroamineTank</li>
        <li>VCEUE_NeutroamineTap</li>
        <li>VCEUE_NeutroamineRefinery</li>
        <li>VCEUE_NeutroamineDrain</li>
      </relatedBuildCommands>
    </building>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>VCHE_PipeNetworks</designationCategory>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>VCEUE_NeutroamineRefining</li>
    </researchPrerequisites>
    <designatorDropdown>Refineries</designatorDropdown>
  </ThingDef>
  <ThingDef ParentName="BuildingBase">
    <defName>VREA_NeutroPipe</defName>
    <drawStyleCategory>Conduits</drawStyleCategory>
    <label>Neutroamine pipe</label>
    <description>Sterile pipes designed to transport usable Neutroamine from deepchem refineries to Neutroamine tanks for storage or to Neutroamine-consuming structures for automatic refilling.</description>
    <thingClass>PipeSystem.Building_Pipe</thingClass>
    <category>Building</category>
    <tickerType>Rare</tickerType>
    <graphicData>
      <texPath>Things/Building/Linked/PurplePipe</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <linkType>Basic</linkType>
      <linkFlags>
        <li>Custom7</li>
      </linkFlags>
      <damageData>
        <rect>(0,0.35,1,0.3)</rect>
      </damageData>
    </graphicData>
    <uiIconPath>Things/Building/Linked/NeutroaminePipes_MenuIcon</uiIconPath>
    <uiIconScale>0.7</uiIconScale>
    <building>
      <ai_chillDestination>false</ai_chillDestination>
      <isInert>true</isInert>
      <isEdifice>false</isEdifice>
      <blueprintGraphicData>
        <texPath>Things/Building/Linked/PipeAtlas</texPath>
      </blueprintGraphicData>
      <relatedBuildCommands>
        <li>VCEUE_NeutroamineRefinery</li>
        <li>VREA_NeutroPipe</li>
        <li>VREA_SubterraneanNeutroPipe</li>
        <li>VCEUE_NeutroamineValve</li>
        <li>VCEUE_NeutroamineTank</li>
        <li>VCEUE_NeutroamineTap</li>
        <li>VCEUE_NeutroamineRefinery</li>
        <li>VCEUE_NeutroamineDrain</li>
      </relatedBuildCommands>
    </building>
    <drawerType>MapMeshOnly</drawerType>
    <altitudeLayer>Conduits</altitudeLayer>
    <passability>Standable</passability>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <statBases>
      <MaxHitPoints>40</MaxHitPoints>
      <WorkToBuild>35</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <costList>
      <Silver>2</Silver>
    </costList>
    <comps>
      <li Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VREA_NeutroamineNet</pipeNet>
      </li>
      <li Class="PipeSystem.CompProperties_SpillWhenDamaged">
        <spillEachTicks>600</spillEachTicks>
        <startAtHitPointsPercent>0.333</startAtHitPointsPercent>
        <amountToDraw>1</amountToDraw>
        <filthMaxSpawnRadius>1</filthMaxSpawnRadius>
        <chooseFilthFrom>
          <li>Filth_Neutroamine</li>
        </chooseFilthFrom>
      </li>
    </comps>
    <placeWorkers>
      <li>PipeSystem.PlaceWorker_Pipe</li>
    </placeWorkers>
    <rotatable>false</rotatable>
    <selectable>true</selectable>
    <neverMultiSelect>true</neverMultiSelect>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <designationCategory>VCHE_PipeNetworks</designationCategory>
    <constructEffect>ConstructMetal</constructEffect>
    <clearBuildingArea>false</clearBuildingArea>
    <researchPrerequisites>
      <li>VCEUE_NeutroamineRefining</li>
    </researchPrerequisites>
    <designatorDropdown>Pipes</designatorDropdown>
  </ThingDef>


      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3.5,3.5)</drawSize>
      <drawOffsetEast>(-0.2,2,0.1)</drawOffsetEast>
      <drawOffsetWest>(0.2,2,0.1)</drawOffsetWest>
      <drawOffsetNorth>(0,2,-0.2)</drawOffsetNorth>
      <drawOffsetSouth>(0,2,0.2)</drawOffsetSouth>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <rotatable>true</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>2500</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(3,2)</size>
    <costList>
      <Steel>200</Steel>
      <ComponentIndustrial>4</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="PipeSystem.CompProperties_AdvancedResourceProcessor">
        <showBufferInfo>false</showBufferInfo>
        <notWorkingKey>VCEUE_CantRefineFSX</notWorkingKey>
        <progressBarOffset>(0, 0, -0.65)</progressBarOffset>
        <showResultItem>true</showResultItem>
        <processes>
          <li>VCEUE_ChemfuelIntoDye</li>
          <li>VCEUE_ChemfuelIntoDyeTierII</li>
          <li>VCEUE_ChemfuelIntoDyeTierIII</li>
          <li MayRequire="ludeon.rimworld.biotech">VCEUE_WasteIntoDye</li>
          <li MayRequire="ludeon.rimworld.biotech">VCEUE_WasteIntoDyeTierII</li>
          <li MayRequire="ludeon.rimworld.biotech">VCEUE_WasteIntoDyeTierIII</li>
        </processes>
        <!-- <soundAmbient>VCHE_RefineryAmbient</soundAmbient> -->
      </li>
      <li Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VCEUE_DyeNet</pipeNet>
      </li>
      <li Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VCHE_ChemfuelNet</pipeNet>
      </li>
      <li Class="PipeSystem.CompProperties_Resource">
        <pipeNet>VREA_NeutroamineNet</pipeNet>
      </li>
      <li Class="PipeSystem.CompProperties_Resource" MayRequire="ludeon.rimworld.biotech">
        <pipeNet>VCEUE_ToxicWasteNet</pipeNet>
      </li>

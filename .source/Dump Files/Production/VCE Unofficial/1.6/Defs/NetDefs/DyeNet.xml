    <building>
      <relatedBuildCommands>
        <li>VCEUE_DyePipe</li>
        <li>VCEUE_UndergroundDyePipe</li>
        <li>VCEUE_DyeValve</li>
        <li>VCEUE_DyeTank</li>
        <li>VCEUE_DyeTap</li>

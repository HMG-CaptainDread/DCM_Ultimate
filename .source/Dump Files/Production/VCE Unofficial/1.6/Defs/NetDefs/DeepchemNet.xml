<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>VCEUE_RawPlasteelRefinery</defName>
    <label>Raw Plasteel Refinery</label>
    <description>A complex machine capable of refining raw plasteel from deepchem delivered via deepchem pipes into raw, curable plasteel.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Building/Production/VCEUE_RawPlasteelRefinery/VCEUE_RawPlasteelRefinery</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,4)</drawSize>
      <drawOffset>(0,2,0.175)</drawOffset>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>2500</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(3,3)</size>
    <costList>
      <Steel>200</Steel>
      <ComponentIndustrial>4</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="PipeSystem.CompProperties_ResourceProcessor">
        <pipeNet>VCHE_DeepchemNet</pipeNet>
        <showBufferInfo>false</showBufferInfo>
        <results>
          <li>
            <countNeeded>200</countNeeded>
            <eachTicks>12000</eachTicks>
            <thing>VCEUE_RawPlasteel</thing>
            <thingCount>20</thingCount>
          </li>
        </results>
        <!-- <soundAmbient>VCHE_RefineryAmbient</soundAmbient> -->
        <notWorkingKey>VCEUE_CantRefine</notWorkingKey>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>5000</basePowerConsumption>
        <shortCircuitInRain>false</shortCircuitInRain>
      </li>
    </comps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>VCHE_PipeNetworks</designationCategory>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>VCEUE_RawPlasteelRefining</li>
    </researchPrerequisites>
    <designatorDropdown>Refineries</designatorDropdown>
    <building>
        <buildingSizeCategory>Medium</buildingSizeCategory>
        <relatedBuildCommands>
            <li>VCEUE_Autoclave</li>
            <li>VCEUE_AdvancedAutoWeavery</li>
            <li>PS_DeepchemRefinery</li>
            <li>VCHE_DeepchemPipe</li>
            <li>VCHE_UndergroundDeepchemPipe</li>
            <li>VCHE_DeepchemValve</li>
            <li>PS_DeepchemTank</li>
            <li>VCHE_DeepchemTap</li>
            <li>VCHE_DeepchemDrain</li>
        </relatedBuildCommands>
    </building>
  </ThingDef>
  <ThingDef ParentName="BuildingBase">
    <defName>VCEUE_AdvancedAutoWeavery</defName>
    <label>Advanced Automatic Weavery</label>
    <description>A complex machine capable of automatically weaving a very plastic precursor to fibers.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Building/Production/VCEUE_ThreadMachine/VCEUE_ThreadMachine</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(4,4)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>2500</WorkToBuild>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(3,3)</size>
    <costList>
      <Steel>200</Steel>
      <ComponentIndustrial>4</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="PipeSystem.CompProperties_ResourceProcessor">
        <pipeNet>VCHE_DeepchemNet</pipeNet>
        <showBufferInfo>false</showBufferInfo>
        <results>
          <li>
            <countNeeded>200</countNeeded>
            <eachTicks>16000</eachTicks>
            <thing>VCEUE_RawThread</thing>
            <thingCount>10</thingCount>
          </li>
        </results>
        <!-- <soundAmbient>VCHE_RefineryAmbient</soundAmbient> -->
        <notWorkingKey>VCEUE_CantRefine</notWorkingKey>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>3000</basePowerConsumption>
        <shortCircuitInRain>false</shortCircuitInRain>
      </li>
    </comps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>VCHE_PipeNetworks</designationCategory>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>VCEUE_SynthreadRefining</li>
    </researchPrerequisites>
    <designatorDropdown>Refineries</designatorDropdown>
    <building>
        <buildingSizeCategory>Medium</buildingSizeCategory>
        <relatedBuildCommands>
            <li>VCEUE_Autoweave</li>
            <li>VCEUE_RawPlasteelRefinery</li>
            <li>PS_DeepchemRefinery</li>
            <li>VCHE_DeepchemPipe</li>
            <li>VCHE_UndergroundDeepchemPipe</li>
            <li>VCHE_DeepchemValve</li>
            <li>PS_DeepchemTank</li>
            <li>VCHE_DeepchemTap</li>
            <li>VCHE_DeepchemDrain</li>
        </relatedBuildCommands>
    </building>
  </ThingDef>

</Defs>
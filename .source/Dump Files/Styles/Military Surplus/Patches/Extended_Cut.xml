<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>CE: Armors - Extended Cut</li>
		</mods>
		<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs</xpath>
				<value>
            <ThingStyleDef>
              <defName>Milsurp_CompositeVestPockets</defName>
              <wornGraphicPath>Things/ApparelStyles/CompositeVestPocketsCamo/CompositeVestPocketsCamo</wornGraphicPath>
              <graphicData>
                <texPath>Things/ApparelStyles/CompositeVestReducedCamo/CompositeVestReducedCamo</texPath>
                <graphicClass>Graphic_Single</graphicClass>
              </graphicData>
            </ThingStyleDef>

            <ThingStyleDef>
              <defName>Milsurp_CompositeVestReducedCamo</defName>
              <wornGraphicPath>Things/ApparelStyles/CompositeVestReducedCamo/CompositeVestReducedCamo</wornGraphicPath>
              <graphicData>
                <texPath>Things/ApparelStyles/CompositeVestReducedCamo/CompositeVestReducedCamo</texPath>
                <graphicClass>Graphic_Single</graphicClass>
              </graphicData>
            </ThingStyleDef>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/StyleCategoryDef[defName="Milsurp"]/thingDefStyles</xpath>
				<value>
					<li>
						<thingDef>CEAEx_CompositeVestReduced</thingDef>
						<styleDef>Milsurp_CompositeVestReducedCamo</styleDef>
					</li>
					<li>
						<thingDef>CEAEx_CompositeVestPockets</thingDef>
						<styleDef>Milsurp_CompositeVestPockets</styleDef>
					</li>
				</value>
			</li>
		</operations>
		</match>
  </Operation>    
	
	  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>Combat Extended Armors</li>
		</mods>
		<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs</xpath>
				<value>
            <ThingStyleDef>
              <defName>Milsurp_Jumpsuit</defName>
              <wornGraphicPath>Things/ApparelStyles/JumpsuitCamo/JumpsuitCamo</wornGraphicPath>
              <graphicData>
                <texPath>Things/ApparelStyles/JumpsuitCamo/JumpsuitCamo</texPath>
                <graphicClass>Graphic_Single</graphicClass>
              </graphicData>
            </ThingStyleDef>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/StyleCategoryDef[defName="Milsurp"]/thingDefStyles</xpath>
				<value>
					<li>
						<thingDef>CE_Armor_Jumpsuit</thingDef>
						<styleDef>Milsurp_Jumpsuit</styleDef>
					</li>
				</value>
			</li>
		</operations>
		</match>
  </Operation>




</Patch>
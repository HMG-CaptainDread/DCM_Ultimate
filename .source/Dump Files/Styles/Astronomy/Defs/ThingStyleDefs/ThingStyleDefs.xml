  <ThingStyleDef Name="Floodlight">
    <defName>ASP_FloodLight</defName>
    <label>flood light</label>
    <graphicData>
      <texPath>Things/Building/BuildingStyles/Astronomy/FloodLight</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>2.5</drawSize>
      <addTopAltitudeBias>true</addTopAltitudeBias>
      <shadowData>
        <volume>(0.2, 0.7, 0.2)</volume>
        <offset>(0, 0, -0.2)</offset>
      </shadowData>
    </graphicData>
    </ThingStyleDef>
	

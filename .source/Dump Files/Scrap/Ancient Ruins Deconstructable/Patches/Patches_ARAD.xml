<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods><li>Ancient urban ruins</li></mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_AcientConcrete"]/building/isTargetable</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_AcientConcrete"]/building/ai_neverTrashThis</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_AcientConcrete"]/building/deconstructible</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_AcientConcrete"]/building/alwaysDeconstructible</xpath>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_AcientConcrete"]</xpath>
					<value>
						<statBases>
							<MeditationFocusStrength>0.24</MeditationFocusStrength>
						</statBases>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_AcientConcrete"]</xpath>
					<value>
						<costList>
							<Steel>6</Steel>
						</costList>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_LoadBearing"]</xpath>
					<value>
						<statBases>
							<MeditationFocusStrength>0.24</MeditationFocusStrength>
						</statBases>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_LoadBearing"]</xpath>
					<value>
						<costList>
							<Steel>6</Steel>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_LoadBearing"]/building/isTargetable</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_LoadBearing"]/building/ai_neverTrashThis</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_LoadBearing"]/building/deconstructible</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_LoadBearing"]/building/alwaysDeconstructible</xpath>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_Concrete"]</xpath>
					<value>
						<statBases>
							<MeditationFocusStrength>0.24</MeditationFocusStrength>
						</statBases>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_Concrete"]</xpath>
					<value>
						<costList>
							<Steel>6</Steel>
						</costList>
					</value>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_Concrete"]/building/isTargetable</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_Concrete"]/building/ai_neverTrashThis</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_Concrete"]/building/deconstructible</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_Wall_Atlas_Concrete"]/building/alwaysDeconstructible</xpath>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="AM_NonfunctionalBuilding_Impassable"]/building</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="AM_Building_Atrium"]/building/deconstructible</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="AM_Building_Atrium"]/building/claimable</xpath>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_Effigy"]</xpath>
					<value>
						<statBases>
							<Beauty>320</Beauty>
						</statBases>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="AM_ParterreA" or defName="AM_ParterreB" or defName="AM_ParterreC"]</xpath>
					<value>
						<statBases>
							<Beauty>20</Beauty>
						</statBases>
					</value>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="AM_NonDeconstructibleAncientBuildingBase"]/building/deconstructible</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="AM_NonDeconstructibleAncientBuildingBase"]/building/claimable</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="AM_Ruins"]/building/claimable</xpath>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_BlockedElevators" or defName="AM_BlockedElevatorsD" or defName="AM_BlockedElevatorsA" or defName="AM_BlockedElevatorsDA"]/building/claimable</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_BlockedElevators" or defName="AM_BlockedElevatorsD" or defName="AM_BlockedElevatorsA" or defName="AM_BlockedElevatorsDA"]/building/deconstructible</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="AM_BlockedElevators" or defName="AM_BlockedElevatorsD" or defName="AM_BlockedElevatorsA" or defName="AM_BlockedElevatorsDA"]/building/alwaysDeconstructible</xpath>
				</li>
			</operations>
		</match>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods><li>Ancient hydroponic farm facilities</li></mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="AncientFacilities"]/building/deconstructible</xpath>
				</li>
			</operations>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods><li>Ancient mining industry</li></mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@Name="BreadMoAM_AncientMiningFacilities"]/building/deconstructible</xpath>
				</li>
			</operations>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods><li>The Dead Man's Switch - AncientCorps</li></mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<!-- Misc/All -->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[@Name="DMSAC_BaseBuilding"]/building/claimable</xpath>
					<value><claimable>true</claimable></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[@Name="DMSAC_BaseBuilding"]/building/repairable</xpath>
					<value><repairable>true</repairable></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[@Name="DMSAC_BaseBuilding"]/building/deconstructible</xpath>
					<value><deconstructible>true</deconstructible></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[@Name="DMSAC_BaseBuilding"]/building/neverBuildable</xpath>
					<value><neverBuildable>false</neverBuildable></value>
				</li>
				<!-- Wall -->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="DMSAC_Wall_A" or defName="DMSAC_Wall_B" or defName="DMSAC_Wall_C"]/building/isTargetable</xpath>
					<value><isTargetable>true</isTargetable></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="DMSAC_Wall_A" or defName="DMSAC_Wall_B" or defName="DMSAC_Wall_C"]/building/ai_neverTrashThis</xpath>
					<value><ai_neverTrashThis>false</ai_neverTrashThis></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="DMSAC_Wall_A" or defName="DMSAC_Wall_B" or defName="DMSAC_Wall_C"]/building/deconstructible</xpath>
					<value><deconstructible>true</deconstructible></value>
				</li>
				<!--li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="DMSAC_Wall_A" or defName="DMSAC_Wall_B" or defName="DMSAC_Wall_C"]/building/alwaysDeconstructible</xpath>
					<value><alwaysDeconstructible>true</alwaysDeconstructible></value>
				</li-->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[@Name="DMSAC_Wall"]/statBases</xpath>
					<value><MaxHitPoints>700</MaxHitPoints></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[@Name="DMSAC_Wall"]/useHitPoints</xpath>
					<value><useHitPoints>true</useHitPoints></value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[@Name="DMSAC_Wall"]</xpath>
					<value><costList><Steel>6</Steel></costList>
					</value>
				</li>
				<!-- LootBox -->
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[@Name="DMSAC_BaseLootBox"]/building/deconstructible</xpath>
					<value><deconstructible>true</deconstructible></value>
				</li>
				<!-- Deactive Mech -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[@Name="BaseDeactivatedMech"]/building</xpath>
					<value><deconstructible>false</deconstructible></value>
				</li>
				<!-- Lift Shaft -->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="DMSAC_Building_LiftShaft" or defName="DMSAC_Building_LiftShaft_Bottom" or defName="DMSAC_Building_LiftShaft_Bottom_B"]</xpath>
					<value>
						<building>
							<isTargetable>false</isTargetable>
							<ai_neverTrashThis>true</ai_neverTrashThis>
							<deconstructible>false</deconstructible>
							<alwaysDeconstructible>false</alwaysDeconstructible>
						</building>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationSequence">
	<success>Normal</success>
		<operations>
			<!-- costList -->
			<!-- Ancient Exostrider Head, AncientExostriderCannon -->
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[defName="AncientExostriderHead"]</xpath>
				<order>Append</order>
				<value>
					<costList>
						<ChunkSlagSteel>4</ChunkSlagSteel>
						<Steel>20</Steel>
					</costList>
				</value>
			</li>
		<!-- Ancient Exostrider Leg -->
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[defName="AncientExostriderLeg"]</xpath>
				<order>Append</order>
				<value>
					<costList>
						<ChunkSlagSteel>3</ChunkSlagSteel>
						<Steel>40</Steel>
					</costList>
				</value>
			</li>
		<!-- Ancient Toxifier Generator -->
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[defName="AncientToxifierGenerator"]</xpath>
				<order>Append</order>
				<value>
					<costList>
						<ChunkSlagSteel>2</ChunkSlagSteel>
						<Steel>12</Steel>
						<ComponentIndustrial>1</ComponentIndustrial>
					</costList>
				</value>
			</li>
		<!-- Ancient Band Node -->
			<li Class="PatchOperationAdd">
				<xpath>*/ThingDef[defName="AncientBandNode"]</xpath>
				<order>Append</order>
				<value>
					<costList>
						<ChunkSlagSteel>4</ChunkSlagSteel>
						<Steel>20</Steel>
						<ComponentIndustrial>1</ComponentIndustrial>
					</costList>
				</value>
			</li>
		</operations>
</Operation>

</Patch>
<Defs>

<!-- Conduits into existing hull was in here, but was removed for balance so some walls can remain unpowered.-->

<!-- Powered Hull, needs a C# assembly to have diagonals probably (I suck at C# hence why this mod is xml)-->
  <ThingDef ParentName="Wall">
    <defName>GravshipHullPowered</defName>
    <label>powered gravship hull</label>
    <designationCategory>Odyssey</designationCategory>
    <description>A strong airtight wall that prevents oxygen leaks in the vacuum of space. The wall connects directly into the underlying substructure, meaning it can only be built on a gravship. This version of the wall has conduit built into it.</description>
    <uiOrder>1800</uiOrder>
    <uiIconPath>Things/Building/Gravship/GravshipHullPowered_MenuIcon</uiIconPath>
    <graphicData>
      <texPath>Things/Building/Linked/GravshipHull/GravshipHull_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <researchPrerequisites>
      <li>BasicGravtech</li>
    </researchPrerequisites>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>300</WorkToBuild>
      <Flammability>0</Flammability>
    </statBases>
    <costList>
    <Steel>1</Steel>
    </costList>
    <costStuffCount>7</costStuffCount>
	<stealable>false</stealable>
    <building>
      <isInert>true</isInert>
      <isStuffableAirtight>true</isStuffableAirtight>
      <isAirtight>true</isAirtight>
      <canExchangeVacuum>false</canExchangeVacuum>
      <relatedBuildCommands Inherit="False">
        <li>PowerConduit</li>
        <li>GravshipHull</li>
        <li>GravshipHullArmor</li>
        <li>GravshipHullSolar</li>
        <li>VacBarrier</li>
        <li>VacAirlock</li>
        <li>OxygenPump</li>
        <li>HeatPump</li>
        <li>ColdPump</li>
        <li>VacPump</li>
        <li>Vent</li>
      </relatedBuildCommands>
      <relatedTerrain>
        <li>Substructure</li>
      </relatedTerrain>
    </building>
    <stuffCategories Inherit="False">
      <li>Metallic</li>
    </stuffCategories>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <comps Inherit="False"/>
    <comps>
        <li Class="CompProperties_Power">
        <compClass>CompPowerTransmitter</compClass>
        <transmitsPower>true</transmitsPower>
        </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_OnSubstructure</li>
    </placeWorkers>
  </ThingDef>


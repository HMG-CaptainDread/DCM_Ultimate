  <ThingDef ParentName="GravshipComponentBase">
    <defName>SignalJammerSmall</defName>
    <label>small signal jammer</label>
	<designatorDropdown>Gravship_SignalJammers</designatorDropdown>
    <description>A signal emitter that disguises the electromagnetic signature of your gravship, allowing it to approach heavily-defended landing sites.</description>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Things/Building/SignalJammer/SignalJammer</texPath>
      <drawSize>(1, 1)</drawSize>
    </graphicData>
    <statBases>
      <MaxHitPoints>325</MaxHitPoints>
      <Flammability>0.3</Flammability>
      <Beauty>-10</Beauty>
      <WorkToBuild>25000</WorkToBuild>
      <Mass>50</Mass>
    </statBases>
    <size>(1, 1)</size>
	<stealable>false</stealable>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <resourcesFractionWhenDeconstructed>1</resourcesFractionWhenDeconstructed>
    <fillPercent>0.6</fillPercent>
    <passability>PassThroughOnly</passability>
    <pathCost>20</pathCost>
    <canOverlapZones>false</canOverlapZones>
    <designationCategory>Odyssey</designationCategory>
    <costList>
      <Gravcore>1</Gravcore>
      <Steel>100</Steel>
      <GravlitePanel>40</GravlitePanel>
      <ComponentSpacer>4</ComponentSpacer>
    </costList>
    <researchPrerequisites>
      <li>AdvancedGravtech</li>
    </researchPrerequisites>
    <terrainAffordanceNeeded>Substructure</terrainAffordanceNeeded>
    <comps>
      <li Class="CompProperties_GravshipFacility">
        <requiresLOS>false</requiresLOS>
        <componentTypeDef>SignalJammer</componentTypeDef>
        <maxSimultaneous>9999</maxSimultaneous>
		<showMaxSimultaneous>false</showMaxSimultaneous>
        <maxDistance>500</maxDistance>
      </li>
    </comps>
    <uiOrder>2400</uiOrder>
  </ThingDef>
  
  
  
  <!-- Small Fuel Optimizer / Fuel Control Unit -->
  <!-- Replaced with Fuel Control Unit for 3% which is a better balanced number, fuel optimizer to be locked behind traders since 15% is big. Un-limiting these has caused an imbalance to play around. -->
  <ThingDef ParentName="GravshipComponentBase">
    <defName>GravshipFuelControlUnit</defName>
    <label>fuel control unit</label>
    <description>A gravtech unit that allows the grav engine lift to coordinate more efficiently with traditional thrusters. It reduces the fuel needed for gravship flight.</description>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Things/Building/Gravship/Gravship_FuelControlUnit</texPath>
      <drawSize>(2, 2)</drawSize>
    </graphicData>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>0.3</Flammability>
      <Beauty>-10</Beauty>
      <Mass>50</Mass>
    </statBases>
    <costList>
      <Steel>50</Steel>
      <ComponentIndustrial>2</ComponentIndustrial>
      <ShuttleEngine>1</ShuttleEngine>
    </costList>
    <researchPrerequisites>
      <li>OrbitalTech</li>
      <li>StandardGravtech</li>
    </researchPrerequisites>
    <size>(1, 1)</size>
    <fillPercent>0.4</fillPercent>
    <passability>PassThroughOnly</passability>
    <pathCost>20</pathCost>
    <canOverlapZones>false</canOverlapZones>
    <designationCategory>Odyssey</designationCategory>
    <terrainAffordanceNeeded>Substructure</terrainAffordanceNeeded>
    <comps>
      <li Class="CompProperties_GravshipFacility">
        <fuelSavingsPercent>0.03</fuelSavingsPercent>
        <requiresLOS>false</requiresLOS>
        <maxSimultaneous>9999</maxSimultaneous>
		<showMaxSimultaneous>false</showMaxSimultaneous>
        <maxDistance>500</maxDistance>
      </li>
    </comps>
    <tradeTags>
      <li>GravshipUpgrade</li>
    </tradeTags>
    <tradeability>All</tradeability>
  </ThingDef>
  
 
  
  <!-- Gravship sensors / Orbital Scanners -->
	<DesignatorDropdownGroupDef>
		<defName>Gravship_Scanners</defName>
	</DesignatorDropdownGroupDef>
  
  <ThingDef ParentName="BuildingBase">
    <defName>OrbitalScannerSmall</defName>
    <label>small orbital scanner</label>
	<designatorDropdown>Gravship_Scanners</designatorDropdown>
    <description>A sensor unit that passively scans for irregular objects orbiting the planet. It will periodically detect locations in orbit that can be visited with transport pods, shuttles, or gravships.\n\nThe scanner won't function under a roof. Building multiple orbital scanners does not increase the chance to find new locations.</description>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Things/Building/OrbitalScanner/OrbitalScanner</texPath>
      <drawSize>(1.5, 1.5)</drawSize>
    </graphicData>
    <size>(1,1)</size>
    <tickerType>Normal</tickerType>
    <castEdgeShadows>false</castEdgeShadows>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <WorkToBuild>5000</WorkToBuild>
      <Flammability>0.3</Flammability>
      <Beauty>-8</Beauty>
    </statBases>
    <costList>
      <Steel>90</Steel>
      <ComponentIndustrial>6</ComponentIndustrial>
      <ComponentSpacer>2</ComponentSpacer>
    </costList>
    <building>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>
    </building>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>500</basePowerConsumption>
      </li>
      <li Class="CompProperties_Breakdownable"/>
      <li Class="CompProperties_Flickable"/>
      <li>
        <compClass>CompOrbitalScanner</compClass>
      </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_NotUnderRoof</li>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
    <altitudeLayer>Building</altitudeLayer>
    <canBeUsedUnderRoof>false</canBeUsedUnderRoof>
	<stealable>false</stealable>
    <fillPercent>0.40</fillPercent>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <designationCategory>Odyssey</designationCategory>
    <researchPrerequisites>
      <li>OrbitalTech</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <uiOrder>2500</uiOrder>
  </ThingDef>
  
  
  

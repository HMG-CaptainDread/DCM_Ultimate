 <!-- Mining charge -->
  <ThingDef ParentName="BuildingBase">
    <defName>MiningCharge</defName>
    <label>mining charge</label>
	<designatorDropdown>MiningCharges</designatorDropdown>
    <description>An explosive device used to quickly blast through rock. It is made from refined chemfuel. The explosive is a bit more powerful than those sometimes found on asteroids.</description>
    <graphicData>
      <texPath>Things/Building/Gravship/MiningCharge</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
      <shadowData>
        <volume>(0.9, 0.5, 0.9)</volume>
      </shadowData>
    </graphicData>
    <uiIconPath>Things/Building/Gravship/MiningCharge</uiIconPath>
    <uiIconScale>1.0</uiIconScale>
    <minifiedDef>MinifiedThing</minifiedDef>
    <size>(1,1)</size>
    <designationCategory>Odyssey</designationCategory>
    <researchPrerequisites>
      <li>Mortars</li>
    </researchPrerequisites>
	<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <fillPercent>0.5</fillPercent>
    <rotatable>false</rotatable>
    <passability>PassThroughOnly</passability>
    <tickerType>Normal</tickerType>
    <pathCost>50</pathCost>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <building>
      <alwaysUninstallable>true</alwaysUninstallable>
      <expandHomeArea>false</expandHomeArea>
    </building>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <costList>
      <Chemfuel>15</Chemfuel>
    </costList>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Mass>10</Mass>
    </statBases>
    <comps>
      <li Class="CompProperties_Explosive">
        <wickTicks>240</wickTicks>
        <explosiveRadius>4.6</explosiveRadius>
        <explosiveDamageType>MiningBomb</explosiveDamageType>
        <explodeOnKilled>true</explodeOnKilled>
        <startWickHitPointsPercent>0.99</startWickHitPointsPercent>
        <extraInspectStringKey>ExplosionOnDamageWarning</extraInspectStringKey>
        <preExplosionSpawnSingleThingDef>Filth_BlastMark</preExplosionSpawnSingleThingDef>
      </li>
      <li Class="CompProperties_Usable">
        <useJob>TriggerObject</useJob>
        <useLabel>Trigger {0_label}</useLabel>
      </li>
      <li Class="CompProperties_UseEffect">
        <compClass>CompUseEffect_StartWick</compClass>
      </li>
    </comps>
  </ThingDef>


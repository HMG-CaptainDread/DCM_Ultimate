<Defs>

  
  <!-- XL Regular Thruster, loads first for menu UI alphabetical order-->
  <ThingDef ParentName="ThrusterBase">
    <defName>XLMiniGravshipThruster</defName>
    <label>capital thruster</label>
    <description>An advanced chemfuel thruster with a heat-resistant vacstone liner. It greatly increases the range of a gravship. Thrusters require an unobstructed area free of large buildings and gravship substructure.\n\nRequires vacstone blocks, which can be obtained from orbital asteroids.</description>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Things/Building/Gravship/XLMiniGravshipThruster</texPath>
      <drawSize>(3.3, 6.6)</drawSize>
      <shadowData>
        <volume>(1.8, 0.75, 1.8)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <MaxHitPoints>1500</MaxHitPoints>
      <Flammability>0</Flammability>
      <Beauty>-10</Beauty>
      <WorkToBuild>34800</WorkToBuild>
      <Mass>160</Mass>
    </statBases>
	<stealable>false</stealable>
    <size>(3, 6)</size>
    <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
    <costList>
      <Steel>500</Steel>
      <BlocksVacstone>100</BlocksVacstone>
      <ComponentIndustrial>16</ComponentIndustrial>
      <ComponentSpacer>4</ComponentSpacer>
    </costList>
    <costStuffCount>10</costStuffCount>
    <stuffCategories Inherit="False">
      <li>Metallic</li>
    </stuffCategories>
    <researchPrerequisites>
      <li>StandardGravtech</li>
    </researchPrerequisites>
    <comps>
      <li Class="CompProperties_GravshipThruster">
        <statOffsets>
          <GravshipRange>44</GravshipRange>
        </statOffsets>
        <componentTypeDef>Thruster</componentTypeDef>
        <exclusionAreaSize>(3, 0, 7)</exclusionAreaSize>
        <exclusionAreaOffset>(-1, 0, -9)</exclusionAreaOffset>
        <directionInfluence>30</directionInfluence>
        <flameSize>4.0</flameSize>
        <flameOffsetsPerDirection>
          <li>(0, 0, 1.0)</li>
          <li>(0, 0, 0.5)</li>
          <li>(0, 0, 0.5)</li>
          <li>(0, 0, 0.5)</li>
        </flameOffsetsPerDirection>
        <flameShaderType>MoteGlow</flameShaderType>
        <flameShaderParameters>
          <_MainTex>/Things/Mote/LargeThruster_Burn</_MainTex>
        </flameShaderParameters>
        <requiresLOS>false</requiresLOS>
        <showMaxSimultaneous>false</showMaxSimultaneous>
        <maxSimultaneous>9999</maxSimultaneous>
        <maxDistance>1000</maxDistance>
        <exhaustSettings>
          <exhaustFleckDef>GravshipThrusterExhaust</exhaustFleckDef>
          <emissionsPerSecond>40.0</emissionsPerSecond>
          <spawnRadiusRange>0.0~0.5</spawnRadiusRange>
          <velocity>(0.0,0.0,-20.0)</velocity>
          <velocityRotationRange>-10.0~10.0</velocityRotationRange>
          <velocityMultiplierRange>0.8~1.2</velocityMultiplierRange>
          <rotationOverTimeRange>-180.0~180.0</rotationOverTimeRange>
          <scaleRange>3.0~5.0</scaleRange>
        </exhaustSettings>
      </li>
      <li Class="CompProperties_Power">
		<compClass>CompPowerTrader</compClass>
		<basePowerConsumption>150</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_Breakdownable" />
    </comps>
    <building>
      <relatedBuildCommands Inherit="False">
    <li>SmallThruster</li>
    <li>LargeThruster</li>
    <li>XLargeThruster</li>
    <li>ModularSmallThruster</li>
    <li>ModularLargeThruster</li>
    <li>ModularXLargeThruster</li>
    <li>ModularXLMiniGravshipThruster</li>
      </relatedBuildCommands>
      <relatedTerrain>
        <li>Substructure</li>
      </relatedTerrain>
				</building>
    <uiOrder>2200</uiOrder>
  </ThingDef>
  
  

<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <!-- FSX Net -->
    <PipeSystem.ProcessDef>
        <defName>VCEUE_ChemfuelIntoFSX</defName>
        <label>Make FSX from Chemfuel</label>
        <ingredients>
            <li>
                <pipeNet>VCHE_ChemfuelNet</pipeNet>
                <thing>Chemfuel</thing>
                <countNeeded>80</countNeeded>
            </li>
            <li>
                <pipeNet>VREA_NeutroamineNet</pipeNet>
                <thing>Neutroamine</thing>
                <countNeeded>10</countNeeded>
            </li>
        </ingredients>
        <ticks>10000</ticks>
        <results>
            <li>
                <pipeNet>VCEUE_FSXNet</pipeNet>
                <thing>FSX</thing>
                <count>10</count>
            </li>
        </results>
        <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
        <researchPrerequisites>
            <li>VCEUE_FSXRefining</li>
        </researchPrerequisites>
    </PipeSystem.ProcessDef>

    <PipeSystem.ProcessDef MayRequire="vanillaexpanded.helixiengas">
        <defName>VCEUE_HelixienGasIntoFSX</defName>
        <label>Make FSX from Helixien</label>
        <ingredients>
            <li>
                <pipeNet>VHGE_HelixienNet</pipeNet>
                <thing>VHGE_Helixien</thing>
                <countNeeded>850</countNeeded>
            </li>
            <li>
                <pipeNet>VREA_NeutroamineNet</pipeNet>
                <thing>Neutroamine</thing>
                <countNeeded>10</countNeeded>
            </li>
        </ingredients>
        <ticks>10000</ticks>
        <results>
            <li>
                <pipeNet>VCEUE_FSXNet</pipeNet>
                <thing>FSX</thing>
                <count>10</count>
            </li>
        </results>
        <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
        <researchPrerequisites>
            <li>VCEUE_FSXRefiningGas</li>
        </researchPrerequisites>
    </PipeSystem.ProcessDef>

    <PipeSystem.ProcessDef>
        <defName>VCEUE_ChemfuelIntoPrometheum</defName>
        <label>Make Prometheum from Chemfuel</label>
        <ingredients>
            <li>
                <pipeNet>VCHE_ChemfuelNet</pipeNet>
                <thing>Chemfuel</thing>
                <countNeeded>3</countNeeded>
            </li>
            <li>
                <pipeNet>VREA_NeutroamineNet</pipeNet>
                <thing>Neutroamine</thing>
                <countNeeded>2</countNeeded>
            </li>
        </ingredients>
        <ticks>24000</ticks>
        <results>
            <li>
                <pipeNet>VCEUE_PrometheumNet</pipeNet>
                <thing>Prometheum</thing>
                <count>3</count>
            </li>
        </results>
        <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
        <researchPrerequisites>
            <li>VCEUE_PrometheumRefining</li>
        </researchPrerequisites>
    </PipeSystem.ProcessDef>
</Defs>
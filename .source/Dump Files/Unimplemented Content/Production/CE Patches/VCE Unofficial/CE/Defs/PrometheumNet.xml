  <ThingDef ParentName="BuildingBase">
    <defName>VCEUE_PrometheumTank</defName>
    <label>Prometheum tank</label>
    <description>An industrial liquid storage tank meant for storing Prometheum delivered via Prometheum pipe network. Tanks can be designated to be siphoned for usable Prometheum as needed. If damaged even lightly the escaping fumes will very swiftly create a gaseous fireball, the size of the resulting explosion will depend on the amount stored within.</description>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Things/Building/Production/PrometheumTank/PrometheumTank</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(4,4)</drawSize>
      <drawOffset>(0,0,0.25)</drawOffset>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <rotatable>False</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <pathCost>50</pathCost>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <WorkToBuild>1200</WorkToBuild>
      <Mass>20</Mass>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(2,3)</size>
    <costList>
      <Plasteel>65</Plasteel>
    </costList>
    <comps>
      <li Class="PipeSystem.CompProperties_ResourceStorage">
        <pipeNet>VCEUE_PrometheumNet</pipeNet>
        <storageCapacity>1500</storageCapacity>
        <barSize>(0.4, 0.175)</barSize>
        <margin>0.15</margin>
        <centerOffset>(0, 0, -0.65)</centerOffset>
        <extractOptions>
          <texPath>UI/Gizmos/ExtractPrometheum</texPath>
          <labelKey>VCEUE_ExtractPrometheum</labelKey>
          <descKey>VCEUE_ExtractPrometheumDesc</descKey>
          <extractAmount>150</extractAmount>
          <thing>Prometheum</thing>
          <ratio>1</ratio>
        </extractOptions>
      </li>
      <li Class="PipeSystem.CompProperties_ExplosiveContent">
        <explosiveMaxRadius>12</explosiveMaxRadius>
        <!-- Vanilla options below -->
        <explosiveDamageType>Flame</explosiveDamageType>
        <destroyThingOnExplosionSize>2</destroyThingOnExplosionSize>
        <startWickHitPointsPercent>0.333</startWickHitPointsPercent>
        <preExplosionSpawnThingDef>Filth_Fuel</preExplosionSpawnThingDef>
        <preExplosionSpawnChance>1</preExplosionSpawnChance>
        <wickTicks>70~150</wickTicks>
      </li>
    </comps>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <designationCategory>VCHE_PipeNetworks</designationCategory>
    <staticSunShadowHeight>0.2</staticSunShadowHeight>
    <constructEffect>ConstructMetal</constructEffect>
    <canOverlapZones>false</canOverlapZones>
    <researchPrerequisites>
      <li>VCEUE_PrometheumRefining</li>
    </researchPrerequisites>
    <designatorDropdown>PipeContainers</designatorDropdown>
    <building>
      <relatedBuildCommands>
        <li>VCEUE_PrometheumPipe</li>
        <li>VCEUE_UndergroundPrometheumPipe</li>
        <li>VCEUE_PrometheumValve</li>
        <li>VCEUE_PrometheumTank</li>
        <li>VCEUE_PrometheumTap</li>
        <li>VCEUE_PrometheumRefinery</li>
        <li>VCEUE_PrometheumDrain</li>
      </relatedBuildCommands>
    </building>
  </ThingDef>


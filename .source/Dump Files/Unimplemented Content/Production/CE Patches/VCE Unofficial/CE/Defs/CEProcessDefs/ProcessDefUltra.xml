<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <!-- FSX Net -->
    <PipeSystem.ProcessDef>
        <defName>VCEUE_ChemfuelIntoFSXTierIII</defName>
        <label>Make FSX from Chemfuel in the fastest way</label>
        <ingredients>
            <li>
                <pipeNet>VCHE_ChemfuelNet</pipeNet>
                <thing>Chemfuel</thing>
                <countNeeded>80</countNeeded>
            </li>
            <li>
                <pipeNet>VREA_NeutroamineNet</pipeNet>
                <thing>Neutroamine</thing>
                <countNeeded>10</countNeeded>
            </li>
        </ingredients>
        <ticks>5000</ticks>
        <results>
            <li>
                <pipeNet>VCEUE_FSXNet</pipeNet>
                <thing>FSX</thing>
                <count>10</count>
            </li>
        </results>
        <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
        <researchPrerequisites>
            <li>VCEUE_FSXRefiningTierIII</li>
        </researchPrerequisites>
    </PipeSystem.ProcessDef>

    <PipeSystem.ProcessDef MayRequire="vanillaexpanded.helixiengas">
        <defName>VCEUE_HelixienGasIntoFSXTierIII</defName>
        <label>Make FSX from Helixien in the fastest way</label>
        <ingredients>
            <li>
                <pipeNet>VHGE_HelixienNet</pipeNet>
                <thing>VHGE_Helixien</thing>
                <countNeeded>850</countNeeded>
            </li>
            <li>
                <pipeNet>VREA_NeutroamineNet</pipeNet>
                <thing>Neutroamine</thing>
                <countNeeded>10</countNeeded>
            </li>
        </ingredients>
        <ticks>5000</ticks>
        <results>
            <li>
                <pipeNet>VCEUE_FSXNet</pipeNet>
                <thing>FSX</thing>
                <count>10</count>
            </li>
        </results>
        <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
        <researchPrerequisites>
            <li>VCEUE_FSXRefiningGasTierIII</li>
        </researchPrerequisites>
    </PipeSystem.ProcessDef>

    <PipeSystem.ProcessDef>
        <defName>VCEUE_ChemfuelIntoPrometheumTierIII</defName>
        <label>Make Prometheum from Chemfuel in the fastest way</label>
        <ingredients>
            <li>
                <pipeNet>VCHE_ChemfuelNet</pipeNet>
                <thing>Chemfuel</thing>
                <countNeeded>3</countNeeded>
            </li>
            <li>
                <pipeNet>VREA_NeutroamineNet</pipeNet>
                <thing>Neutroamine</thing>
                <countNeeded>2</countNeeded>
            </li>
        </ingredients>
        <ticks>12000</ticks>
        <results>
            <li>
                <pipeNet>VCEUE_PrometheumNet</pipeNet>
                <thing>Prometheum</thing>
                <count>3</count>
            </li>
        </results>
        <destroyIngredientsDirectly>true</destroyIngredientsDirectly>
        <researchPrerequisites>
            <li>VCEUE_PrometheumRefiningTierIII</li>
        </researchPrerequisites>
    </PipeSystem.ProcessDef>
</Defs>
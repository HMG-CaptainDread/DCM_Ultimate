<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="XmlExtensions.OptionalPatch"> <key>Visible_AnimaEye</key>
<modId>Seti.Victor.AnimaBodies</modId>
<defaultValue>true</defaultValue>
<caseTrue>
	<Operation Class="XmlExtensions.PatchOperationSafeAdd">
	<xpath>Defs/HediffDef[defName="AnimaEye"]</xpath>
	<value>
		<renderNodeProperties>
			<li Class="PawnRenderNodeProperties_Eye">
				<workerClass>PawnRenderNodeWorker_HediffEye</workerClass>
				<texPath>Things/Pawn/Humanlike/BodyAttachments/AnimaEye/AnimaEye</texPath>
				<parentTagDef>Head</parentTagDef>
				<debugLabel>Base Layer</debugLabel>
				<drawSize>0.21</drawSize>
				<drawData>
					<defaultData>
						<layer>56</layer>
						<offset>(0, 0, -0.25)</offset>
					</defaultData>
					<dataWest>
						<flip>true</flip>
					</dataWest>
				</drawData>
			</li>
		</renderNodeProperties>
	</value>
	</Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/HediffDef[defName="AnimaEye"]</xpath>
		<value>
			<li Class="BodyAttachments.DefModExtension_HediffLayer"/>
		</value>
	</Operation>
</caseTrue>
</Operation>

	<!-- ======= Combat Extended caliber-specific guns (would be redundant in base game) ======= -->	
	<!-- Mounted M2A1 - Marshal -->
	<Vehicles.VehicleTurretDef ParentName="VehicleTurretBase" MayRequireAnyOf="CETeam.CombatExtended,CETeam.CombatExtended_steam">
		<defName>TGBM_Marshal_PintleHMG</defName>
		<label>Mounted Turret</label>
		<description>A heavy machine gun mounted to an enclosed turret.</description>
		
		<turretType>Rotatable</turretType>
		<shotSound>TGBM_Shot_HeavyMachineGun</shotSound>
		
		<projectile>Bullet_50BMG_FMJ</projectile>
		<reloadTimer>7.8</reloadTimer>
		<warmUpTimer>1.3</warmUpTimer>
		<chargePerAmmoCount>1</chargePerAmmoCount>
		
		<magazineCapacity>200</magazineCapacity>
		<genericAmmo>false</genericAmmo>
		
		<autoSnapTargeting>false</autoSnapTargeting>
		<rotationSpeed>2.5</rotationSpeed>
		
		<projectileOffset>1</projectileOffset>
		
		<maxRange>75</maxRange>
		
		<recoil>
			<distanceTotal>0.15</distanceTotal>
			<distancePerTick>0.05</distancePerTick>
			<speedMultiplierPostRecoil>0.1</speedMultiplierPostRecoil>
		</recoil>
		
		<attachProjectileFlag>VVE_Tank</attachProjectileFlag>
		
		<fireModes>
			<li>
				<shotsPerBurst>1</shotsPerBurst>
				<ticksBetweenShots>5</ticksBetweenShots>
				<ticksBetweenBursts>60</ticksBetweenBursts>
				<label>Single</label>
				<texPath>UI/Gizmos/FireRate_Single</texPath>
			</li>
			<li>
				<shotsPerBurst>5</shotsPerBurst>
				<ticksBetweenShots>5</ticksBetweenShots>
				<ticksBetweenBursts>60</ticksBetweenBursts>
				<label>Burst</label>
				<texPath>UI/Gizmos/FireRate_Burst</texPath>
			</li>
			<li>
				<shotsPerBurst>10</shotsPerBurst>
				<ticksBetweenShots>5</ticksBetweenShots>
				<ticksBetweenBursts>60</ticksBetweenBursts>
				<label>Auto</label>
				<texPath>UI/Gizmos/FireRate_Auto</texPath>
			</li>
		</fireModes>

		<graphicData>
			<texPath>Things/Vehicles/Armament/VVE_PintleMG</texPath>
			<graphicClass>Vehicles.Graphic_Turret</graphicClass>
			<shaderType>Cutout</shaderType>
			<drawSize>(2, 2)</drawSize>
		</graphicData>
		

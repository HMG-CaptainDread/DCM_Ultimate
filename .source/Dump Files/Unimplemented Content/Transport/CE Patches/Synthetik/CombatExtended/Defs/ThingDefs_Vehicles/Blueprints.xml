<Defs>

	<Vehicles.VehicleBuildDef ParentName="VehicleBaseBuildable">
		<defName>Sat7_Blueprint</defName>
		<label>Saturn 7 Heavy Tank</label>
		<description>A Heavy Tank fielded by Kaida Corporation. Rarely seen outside their military bays due to its logistical profile.</description>
		<descriptionHyperlinks>
			<CombatExtended.AmmoSetDef>AmmoSet_100mmEnergyCanister</CombatExtended.AmmoSetDef>
			<CombatExtended.AmmoSetDef>AmmoSet_SynthetikLaserCell</CombatExtended.AmmoSetDef>
		</descriptionHyperlinks>
		<designationCategory>VF_Vehicles</designationCategory>
		<graphicData>
			<texPath>Synthetik/Vehicles/HeavyArmor</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(4.5, 6.75)</drawSize>
		</graphicData>
		<statBases>
			<MaxHitPoints>1200</MaxHitPoints>
			<WorkToBuild>50000</WorkToBuild>
			<Flammability>0.05</Flammability>
		</statBases>
		<size>(3, 5)</size>
		<researchPrerequisites>
			<li>AdvancedFabrication</li>
		</researchPrerequisites>
		<costList>
			<Plasteel>700</Plasteel>
			<Steel>2160</Steel>
			<Uranium>300</Uranium>
			<Silver>180</Silver>
			<ComponentIndustrial>48</ComponentIndustrial>
			<ComponentSpacer>24</ComponentSpacer>
			<AIPersonaCore>1</AIPersonaCore>
		</costList>
		<soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
		<thingToSpawn>Sat7_HeavyTankPawn</thingToSpawn>
		<constructionSkillPrerequisite>16</constructionSkillPrerequisite>
	</Vehicles.VehicleBuildDef>


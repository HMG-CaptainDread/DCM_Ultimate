<PipeSystem.ProcessDef>
  <defName>AutoHydroponic_Rice</defName>
  <label>Growing rice</label>
  <description>Grow rice.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 160 rice in 2 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>120000</ticks> <!-- 2 in-game days -->
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>RawRice</thing>
      <count>160</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


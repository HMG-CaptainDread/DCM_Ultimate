<PipeSystem.ProcessDef MayRequire="VanillaExpanded.VBrewE">
  <defName>AutoHydroponic_VBEPlantCoffeeT</defName>
  <label>Growing coffee</label>
  <description>Grow coffee.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 94 coffee in 8.2 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>492000</ticks> 
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>VBE_RawCoffee</thing>
      <count>94</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


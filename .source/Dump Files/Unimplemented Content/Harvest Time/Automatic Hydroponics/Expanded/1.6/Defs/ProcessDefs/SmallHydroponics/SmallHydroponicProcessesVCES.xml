<PipeSystem.ProcessDef MayRequire="VanillaExpanded.VCookESushi">
  <defName>AutoHydroponic_VCESoybeanSm</defName>
  <label>Growing soybean</label>
  <description>Grow soybean.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 120 soybean in 3 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>180000</ticks> 
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>VCE_RawSoybean</thing>
      <count>120</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


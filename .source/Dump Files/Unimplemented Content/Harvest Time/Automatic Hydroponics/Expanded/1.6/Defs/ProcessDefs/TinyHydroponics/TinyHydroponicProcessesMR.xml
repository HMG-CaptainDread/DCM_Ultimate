<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Milira Sun Light Flower Tiny-->
<PipeSystem.ProcessDef MayRequire="Ancot.MiliraRace">
  <defName>AutoHydroponic_MiliraSunLightFlowerT</defName>
  <label>Growing sun blooms</label>
  <description>Grow sun blooms that draw special energy from sunlight for fuel refining.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 10 sun blooms in 14 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>840000</ticks> <!-- 14 in-game days -->
  <ingredients>
    <li>
      <thing>WoodLog</thing>
      <countNeeded>0</countNeeded>
    </li>
  </ingredients>
  <results>
    <li>
      <thing>Milira_SunLightFruit</thing>
      <count>10</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
  <researchPrerequisites>
    <li>Milira_SunLightDaisyPlanting</li>
  </researchPrerequisites>
</PipeSystem.ProcessDef>

</Defs>
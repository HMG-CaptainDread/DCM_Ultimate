<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Chrysanthemum-->

<PipeSystem.ProcessDef>
  <defName>FloralHydroponic_RI_Plant_Chrysanthemum</defName>
  <label>Growing chrysanthemum (from RimImmortal)</label>
  <description>Grow chrysanthemums.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow chrysanthemums (from RimImmortal)</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>120000</ticks> 
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>Hay</thing>
      <count>80</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


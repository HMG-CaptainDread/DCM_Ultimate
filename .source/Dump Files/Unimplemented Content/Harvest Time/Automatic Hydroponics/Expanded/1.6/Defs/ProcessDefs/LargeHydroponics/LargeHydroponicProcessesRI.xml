<PipeSystem.ProcessDef MayRequire="RI.RimImmortal.Living">
  <defName>AutoHydroponic_RI_Plant_Watermelon</defName>
  <label>Growing watermelon</label>
  <description>Grow watermelons.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 50 watermelons in 6 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>360000</ticks> 
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>RI_Resource_Watermelon</thing>
      <count>50</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
  <researchPrerequisites>
            <li>RI_Planting</li>
  </researchPrerequisites>
</PipeSystem.ProcessDef>


<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--VCE_Lettuce-->

<PipeSystem.ProcessDef MayRequire="VanillaExpanded.VPlantsEMore">
  <defName>AutoHydroponic_VCELettuceT</defName>
  <label>Growing lettuce</label>
  <description>Grow lettuce.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 30 lettuce in 2.9 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>174000</ticks> 
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>VCE_RawLettuce</thing>
      <count>30</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


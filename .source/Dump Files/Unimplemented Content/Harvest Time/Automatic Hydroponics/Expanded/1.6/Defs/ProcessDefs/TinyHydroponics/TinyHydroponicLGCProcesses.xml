<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Bell Pepper-->

<PipeSystem.ProcessDef MayRequire="lingluo.giantcrop">
  <defName>AutoHydroponic_LGCBellPepperT</defName>
  <label>Growing bell peppers </label>
  <description>Grow bell peppers.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 32 bell peppers in 4.6 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>276000</ticks> <!--4.6 in-game days -->
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>LGC_RawBellPepper</thing>
      <count>32</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


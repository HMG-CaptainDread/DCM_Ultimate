<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Hemofungus-->

<PipeSystem.ProcessDef MayRequire="nosleep.hemofungus">
  <defName>AutoHydroponic_HFHemofungusT</defName>
  <label>Growing hemofungus</label>
  <description>Grow hemofungus.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 9 hemofungus in 8.3 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>498000</ticks> <!-- 8.3 in-game days -->
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>RawHemofungus</thing>
      <count>9</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
      <researchPrerequisites>
        <li>Devilstrand</li>
      </researchPrerequisites>
</PipeSystem.ProcessDef>

</Defs>

    <stuffProps>
      <categories>
        <li>Fabric</li>
      </categories>
      <commonality>0.15</commonality>
	 <statFactors>
        <Flammability>0.7</Flammability>
        <MaxHitPoints>1.3</MaxHitPoints>
        <Beauty>2.3</Beauty>
      </statFactors>
      <allowColorGenerators>false</allowColorGenerators>
    </stuffProps>
    <thingCategories>
      <li>DyedSynthread</li>
    </thingCategories>
		<tradeTags>
			<li>GD_ColoredTextile</li>
		</tradeTags>
    <allowedArchonexusCount>80</allowedArchonexusCount>		
  </ThingDef>



<?xml version="1.0" encoding="utf-8" ?>

<Patch>

  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>Strawberries</li>
		</mods>
		<match Class="PatchOperationSequence"> 
			
			<operations>
			  <li Class="PatchOperationFindMod">
				<mods>
					<li>SeedsPlease</li>
					<li>SeedsPlease: Dizzy Fork</li>
				</mods>
				<match Class="PatchOperationSequence"> 
					<operations>
					  <li Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="VBY_ExtractSeedStrawberryJugust" or defName="VBY_ExtractSeedStrawberryDN"]/ingredients/li/filter/thingDefs</xpath>
						<value>
						  <thingDefs>
							<li>CNS_Strawberries_RawStrawberries</li>
						  </thingDefs>					
						</value>
					  </li>
					  <li Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="VBY_ExtractSeedStrawberryJugust" or defName="VBY_ExtractSeedStrawberryDN"]/fixedIngredientFilter/thingDefs</xpath>
						<value>
						  <thingDefs>
							<li>CNS_Strawberries_RawStrawberries</li>
						  </thingDefs>					
						</value>
					  </li>
					</operations>
				</match>
			  </li>
			  <li Class="PatchOperationFindMod">
				<mods>
					<li>SeedsPlease: Lite</li>
				</mods>
				<match Class="PatchOperationSequence"> 
					<operations>
					  <li Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="VBY_ExtractSeedStrawberryJugustLite" or defName="VBY_ExtractSeedStrawberryDNLite"]/ingredients/li/filter/thingDefs</xpath>
						<value>
						  <thingDefs>
							<li>CNS_Strawberries_RawStrawberries</li>
						  </thingDefs>					
						</value>
					  </li>
					  <li Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="VBY_ExtractSeedStrawberryJugustLite" or defName="VBY_ExtractSeedStrawberryDNLite"]/fixedIngredientFilter/thingDefs</xpath>
						<value>
						  <thingDefs>
							<li>CNS_Strawberries_RawStrawberries</li>
						  </thingDefs>					
						</value>
					  </li>
					  <li Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="VBY_ExtractSeedStrawberryJugustLite" or defName="VBY_ExtractSeedStrawberryDNLite"]/defaultIngredientFilter/thingDefs</xpath>
						<value>
						  <thingDefs>
							<li>CNS_Strawberries_RawStrawberries</li>
						  </thingDefs>					
						</value>
					  </li>
					</operations>
				</match>
			  </li>			   
			  <li Class="PatchOperationReplace"> 
				<xpath>Defs/ThingDef[defName="VBY_Plant_Jugust_Strawberry"]/plant/harvestedThingDef</xpath>
				<value>
					<harvestedThingDef>CNS_Strawberries_RawStrawberries</harvestedThingDef>				
				</value>
			  </li>			   
			  <li Class="PatchOperationReplace"> 
				<xpath>Defs/ThingDef[defName="VBY_Plant_DayNeutral_Strawberry"]/plant/harvestedThingDef</xpath>
				<value>
					<harvestedThingDef>CNS_Strawberries_RawStrawberries</harvestedThingDef>					
				</value>
			  </li> 			   
			</operations>
		</match>
  </Operation> 
  
  
  
</Patch> 
<?xml version="1.0" encoding="utf-8" ?>

<Patch>

  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>SeedsPlease</li>
			<li>SeedsPlease: Dizzy Fork</li>
		</mods>
		<match Class="PatchOperationSequence">          
			<operations> 
			  
			 <li Class="PatchOperationReplace">
				<xpath>Defs/SeedsPlease.SeedDef[defName="Seed_Strawberry"]/label</xpath>
				<value>
					<label>everbearing strawberry seed</label>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/RecipeDef[defName="ExtractSeedStrawberry"]/label</xpath>
				<value>
					<label>extract everbearing strawberry seeds</label>
				</value>
			</li><!--
			<li Class="PatchOperationReplace">
				<xpath>Defs/SeedsPlease.SeedDef[defName="VBY_Seed_StrawberryJugust"]/label</xpath>
				<value><label>Jugust-bearing strawberry seed</label></value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/SeedsPlease.SeedDef[defName="VBY_Seed_StrawberryDN"]/label</xpath>
				<value><label>Day Neutral strawberry seed</label></value>
			</li>-->
			  
			</operations>
		</match>
		<nomatch Class="PatchOperationSequence">          
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>Defs/SeedsPlease.SeedDef[defName="VBY_Seed_StrawberryJugust" or defName="VBY_Seed_StrawberryDN"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[defName="VBY_ExtractSeedStrawberryJugust" or defName="VBY_ExtractSeedStrawberryDN"]</xpath>
				</li>
			</operations>
		</nomatch>
  </Operation>


  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>SeedsPlease: Lite</li>
		</mods>
		<match Class="PatchOperationSequence">          
			<operations> 
			  
			 <li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Seed_Strawberry"]/label</xpath>
				<value>
					<label>everbearing strawberry seeds</label>
				</value>
			</li>
			</operations>
		</match>   
  </Operation>                         
  
</Patch>  
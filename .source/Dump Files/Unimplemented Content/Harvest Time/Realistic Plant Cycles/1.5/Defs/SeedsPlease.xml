<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<SeedsPlease.SeedDef ParentName="SeedBase">
		<defName>VBY_Seed_StrawberryJugust</defName>
		<label>Jugust-Bearing strawberry seeds</label>
		<sources>
		  <li>VBY_Plant_Jugust_Strawberry</li>
		</sources>
		<graphicData>
			<texPath>Things/Item/Seeds/Strawberry</texPath>
		</graphicData>
		<descriptionHyperlinks>
			<ThingDef>VBY_Plant_Jugust_Strawberry</ThingDef>
		</descriptionHyperlinks>
	</SeedsPlease.SeedDef>
	 
	<SeedsPlease.SeedDef ParentName="SeedBase">
		<defName>VBY_Seed_StrawberryDN</defName>
		<label>Day neutral strawberry seeds</label>
		<sources>
			  <li>VBY_Plant_DayNeutral_Strawberry</li>
		</sources>
		<graphicData>
			<texPath>Things/Item/Seeds/Strawberry</texPath>
		</graphicData>
		<descriptionHyperlinks>
			<ThingDef>VBY_Plant_DayNeutral_Strawberry</ThingDef>
		</descriptionHyperlinks>
	</SeedsPlease.SeedDef>

	<RecipeDef ParentName="ExtractSeed">
		<defName>VBY_ExtractSeedStrawberryJugust</defName>
		<label>extract Jugust-bearing strawberry seeds</label>
		<description>Extract seeds from Berries. Will produce Jugust-Bearing Strawberry seeds.</description>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>RawBerries</li>
			  </thingDefs>
			</filter>
			<count>5</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>RawBerries</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
		  <VBY_Seed_StrawberryJugust>3</VBY_Seed_StrawberryJugust>
		</products>
	</RecipeDef>
		  
	<RecipeDef ParentName="ExtractSeed">
	  <defName>VBY_ExtractSeedStrawberryDN</defName>
		<label>extract day-neutral strawberry seeds</label>
		<description>Extract seeds from Berries. Will produce Day-Neutral Strawberry seeds.</description>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>RawBerries</li>
			  </thingDefs>
			</filter>
			<count>5</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>RawBerries</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
		  <VBY_Seed_StrawberryDN>3</VBY_Seed_StrawberryDN>
		</products>
	</RecipeDef>

</Defs>
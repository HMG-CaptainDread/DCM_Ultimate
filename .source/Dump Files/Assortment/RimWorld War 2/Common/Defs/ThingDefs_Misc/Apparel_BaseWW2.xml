<Defs>

<!-- Uniform -->
  <ThingDef ParentName="ApparelMakeableBase" Name="WW2Uniform" Abstract="True">
    <defName>K4G_Apparel_WW2UniformBase</defName>
    <description>A standard infantry uniform used during WW2, does not provide much protection.</description>
    <recipeMaker>
      <researchPrerequisite>K4G_WW2EnlistedUniforms</researchPrerequisite>
    </recipeMaker>
    <techLevel>Industrial</techLevel>
	<costList>
		<Cloth>65</Cloth>
	</costList>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>3200</WorkToMake>
      <Mass>0.5</Mass>
	  <ArmorRating_Sharp>0.072</ArmorRating_Sharp>
	  <ArmorRating_Blunt>0.036</ArmorRating_Blunt>
	  <ArmorRating_Heat>0.036</ArmorRating_Heat>
	  <Insulation_Cold>8.3</Insulation_Cold>
	  <Insulation_Heat>3.2</Insulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>OnSkin</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
	<comps Inherit="False">
	 <li Class="CompProperties_Forbiddable"/>
	 <li>
		<compClass>CompQuality</compClass>
	 </li>
	</comps>
  </ThingDef>
  

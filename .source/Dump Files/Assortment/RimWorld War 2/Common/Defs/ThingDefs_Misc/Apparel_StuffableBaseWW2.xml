<Defs>

<!-- Stuffable Uniform -->  
  <ThingDef ParentName="ApparelMakeableBase" Name="WW2StuffableUniform" Abstract="True">
    <defName>K4G_Apparel_WW2StuffableUniformBase</defName>
    <description>A standard infantry uniform used during WW2, does not provide much protection.</description>
    <recipeMaker>
      <researchPrerequisite>K4G_WW2EnlistedUniforms</researchPrerequisite>
    </recipeMaker>
    <techLevel>Industrial</techLevel>
    <costStuffCount>65</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>3200</WorkToMake>
      <Mass>0.5</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.46</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.18</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>OnSkin</li>
      </layers>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <only>(109,100,77)</only>
        </li>
        <li>
          <only>(96,100,78)</only>
        </li>
        <li>
          <only>(100,98,84)</only>
        </li>
        <li>
          <only>(131,124,95)</only>
        </li>
        <li>
          <only>(126,127,99)</only>
        </li>
        <li>
          <only>(164,147,115)</only>
        </li>
        <li>
          <only>(125,102,76)</only>
        </li>
        <li>
          <only>(104,83,65)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
  

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ProcessorFramework.ProcessDef>
		<defName>Medtimes_CheeseProcess</defName>
		<thingDef>MedTimes_MealCheese</thingDef>
		<ingredientFilter>
		  <thingDefs>
			<li>Milk</li>
		  </thingDefs>
		</ingredientFilter>
		<processDays>4</processDays> <!-- in days, a day is 60,000 ticks -->
		<capacityFactor>1.0</capacityFactor> <!-- do not use too many decimals e.g 0.333 is fine for 1/3 -->
		<efficiency>1.0</efficiency>
		<usesTemperature>true</usesTemperature>
		<temperatureSafe>-1.0~52.0</temperatureSafe>
		<temperatureIdeal>15.0~25.0</temperatureIdeal>
		<ruinedPerDegreePerHour>2.5</ruinedPerDegreePerHour> <!-- percent per degree per hour -->
		<speedBelowSafe>0.1</speedBelowSafe>
		<speedAboveSafe>1.0</speedAboveSafe>
		<sunFactor>1.0~1.0</sunFactor>
		<rainFactor>1.0~1.0</rainFactor>
		<snowFactor>1.0~1.0</snowFactor>
		<windFactor>1.0~1.0</windFactor>
		<unpoweredFactor>0.0</unpoweredFactor>
		<unfueledFactor>0.0</unfueledFactor>
		<powerUseFactor>1.0</powerUseFactor>
		<fuelUseFactor>1.0</fuelUseFactor>
		<filledGraphicSuffix></filledGraphicSuffix>
		<usesQuality>false</usesQuality>
		<qualityDays>(1, 2, 3, 4, 5, 6, 7)</qualityDays>
		<color>(255, 255, 255)</color>
		<customLabel></customLabel>
		<destroyChance>0.0</destroyChance>
		<bonusOutputs>
		</bonusOutputs>
	</ProcessorFramework.ProcessDef>
	
	<ProcessorFramework.ProcessDef>
		<defName>MedTimes_MealJerkedMeats</defName>
		<thingDef>MedTimes_MealJerkedMeats</thingDef>
		<ingredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>MedTimes_MealJerkedMeats</li>
			</disallowedThingDefs>
		</ingredientFilter>
		<processDays>2</processDays>
		<usesTemperature>false</usesTemperature>
	</ProcessorFramework.ProcessDef>

	<ThingDef ParentName="MedTimes_BuildingBase">
		<defName>MedTimes_CheesePress</defName>
		<label>Cheese press</label>
		<description>A press mechanism for coagulating milk into cheese.</description>
		<thingClass>Building</thingClass>
		<uiIconPath>Things/Building/Production/CheesePress/Cheese_Press_Icon</uiIconPath>
		<drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/Production/CheesePress/Cheese_Press</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawRotated>false</drawRotated>
			<damageData>
				<rect>(0.05,0.1,0.9,0.9)</rect>
			</damageData>
			<shadowData>
				<volume>(0.35, 0.35, 0.63)</volume>
			</shadowData>
		</graphicData>
		<minifiedDef>MinifiedThing</minifiedDef>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.45</fillPercent>
		<pathCost>60</pathCost>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<statBases>
			<WorkToBuild>600</WorkToBuild>
			<Mass>10</Mass>
			<MaxHitPoints>100</MaxHitPoints>
		</statBases>
		<stuffCategories>
			<li>Stony</li>
			<li>Woody</li>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>15</costStuffCount>
		<costList>
			<Steel>10</Steel>
			<WoodLog>15</WoodLog>
		</costList>
		<comps>
		  <li Class="ProcessorFramework.CompProperties_Processor">
			<capacity>200</capacity>
			<independentProcesses>true</independentProcesses>
			<parallelProcesses>true</parallelProcesses>
			<dropIngredients>false</dropIngredients>
			<showProductIcon>true</showProductIcon>
			<barOffset>(0.0, 0.0)</barOffset>
			<barScale>(1.0, 1.0)</barScale>
			<colorCoded>false</colorCoded>
			<processes>
			  <li>Medtimes_CheeseProcess</li>
			</processes>
		  </li>
		  <li Class="CompProperties_Forbiddable"/>
		</comps>
		<tickerType>Rare</tickerType>
		<rotatable>false</rotatable>
		<designationCategory>Production</designationCategory>
		<researchPrerequisites>
			<li>MedTimes_Cheese</li>
		</researchPrerequisites>
	</ThingDef>
</Defs>

    <!-- Paperwork -->
    <RecipeDef>
        <defName>MedTimes_MakeLeatherPaper_Recipe</defName>
        <description>Make some paper from leather material.</description>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Tailor</soundWorking>
        <workAmount>12500</workAmount>
        <label>Make paper.</label>
        <jobString>Making some paper.</jobString>
        <researchPrerequisite>MedTimes_Paperwork</researchPrerequisite>
        <unfinishedThingDef>MedTimes_UnfinishedPapers</unfinishedThingDef>

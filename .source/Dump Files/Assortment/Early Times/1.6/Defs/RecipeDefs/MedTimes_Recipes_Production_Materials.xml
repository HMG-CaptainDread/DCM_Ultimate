<Defs>
    <!-- Treated Bricks -->
    <RecipeDef>
        <defName>MedTimes_MakeBricks</defName>
        <label>Make Bricks</label>
        <description>Use clay and heat to make shurdy dried up bricks</description>
        <jobString>Making Bricks.</jobString>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Cremate</soundWorking>
        <recipeUsers>
            <li>Campfire</li>
            <li>MedTimes_Firepit</li>
            <li>MedTimes_StoneHearth</li>
        </recipeUsers>
        <targetCountAdjustment>20</targetCountAdjustment>
        <workAmount>3000</workAmount>
        <researchPrerequisite>MedTimes_Bricks</researchPrerequisite>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>MedTimes_Resource_DryClayBricks</li>
                    </thingDefs>
                </filter>
                <count>30</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>MedTimes_Resource_DryClayBricks</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <MedTimes_Resource_Bricks>20</MedTimes_Resource_Bricks>
        </products>
    </RecipeDef>
	

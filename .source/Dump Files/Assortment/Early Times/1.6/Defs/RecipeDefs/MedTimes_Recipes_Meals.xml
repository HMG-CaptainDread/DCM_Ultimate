<Defs>
	<!-- ================================ Preserved Foods ============================= -->
	<!-- ======= Jerked Meats ====== -->
	<!-- (Medieval) Jerked Meats Single -->
	<RecipeDef Name="DriedFoodRecipe" Abstract="true">
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<workSkill>Cooking</workSkill>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<researchPrerequisite>MedTimes_DriedFood</researchPrerequisite>
		<fixedIngredientFilter>
			<categories/>
			<disallowedThingDefs/>
		</fixedIngredientFilter>
	</RecipeDef>
	<RecipeDef Name="DriedMeatRecipe" ParentName="DriedFoodRecipe" Abstract="true">
		<recipeUsers>
			<li>MedTimes_Smokehouse</li>
		</recipeUsers>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>MedTimes_MealJerkedMeats</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedCategories>
				<li>EggsFertilized</li>
			</disallowedCategories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>Meat_Megaspider</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
	</RecipeDef>
	<RecipeDef ParentName="DriedMeatRecipe">
		<defName>MedTimes_CookMealJerkedMeats</defName>
		<label>Cook Jerked Meats.</label>
		<description>Cooks a somewhat simple food item from a complex process of drying meats over a fire.</description>
		<jobString>Cooking and Jerking Meats.</jobString>
		<workAmount>1200</workAmount>
		<recipeUsers>
			<li>MedTimes_StoneHearth</li>
			<li>MedTimes_Firepit</li>
			<li>Campfire</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
						<li>AnimalProductRaw</li>
					</categories>
				</filter>
				<count>0.5</count>
			</li>
		</ingredients>
		<products>
			<MedTimes_MealJerkedMeats>10</MedTimes_MealJerkedMeats>
		</products>
		<skillRequirements>
			<Cooking>3</Cooking>
		</skillRequirements>
	</RecipeDef>
	<RecipeDef ParentName="DriedMeatRecipe">
		<defName>MedTimes_CookMealJerkedMeatsBulk</defName>
		<label>Bulk Cook 10 Jerked Meats.</label>
		<description>Cooks a somewhat simple food item from a complex process of drying meats over a fire.</description>
		<jobString>Cooking and Jerking Meats.</jobString>
		<workAmount>5000</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
						<li>AnimalProductRaw</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<products>
			<MedTimes_MealJerkedMeats>100</MedTimes_MealJerkedMeats>
		</products>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
	</RecipeDef>

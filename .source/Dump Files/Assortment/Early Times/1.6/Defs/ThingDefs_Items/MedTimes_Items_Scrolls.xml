<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="MedTimes_Scroll" Abstract="True">
    <thingClass>ThingWithComps</thingClass>
    <category>Item</category>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <useHitPoints>true</useHitPoints>
    <selectable>true</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <stackLimit>200</stackLimit>
    <alwaysHaulable>true</alwaysHaulable>
    <drawGUIOverlay>true</drawGUIOverlay>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
    <intricate>true</intricate>	
    <tickerType>Never</tickerType>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>6</DeteriorationRate>
	  <MarketValue>23</MarketValue>	  
      <Mass>0.03</Mass>	  
      <BookQualityFactor>0.7</BookQualityFactor>
	  <BookChairSearchRadius>15</BookChairSearchRadius>
    </statBases>	
    <comps>
      <li Class="CompProperties_Forbiddable"/>
    </comps>	
    <thingCategories>
      <li>ScrollsStuff</li>
    </thingCategories>
    <tradeTags>
		<li>MedTimes_Books</li>
	</tradeTags>	
  </ThingDef>
  
  <ThingDef ParentName="MedTimes_Scroll">
	<defName>NoteGraphicNovel</defName>
    <label>Entertainment note</label>
    <description>Fiction writings for enjoyment. Contains different fairytales, tales, legends, etc.</description>
    <graphicData>
      <texPath>Things/Item/TradeGoods/Notes/GraphicNovels</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <drawSize>(1,1)</drawSize>	  
    </graphicData>
	<statBases>
      <BookQualityFactor>1.35</BookQualityFactor>
    </statBases>
  </ThingDef>
  
  <ThingDef ParentName="MedTimes_Scroll">
	<defName>NoteMeleeCombat</defName>
    <label>Combat note</label>
    <description>Describes different melee stances and melee combat strategies.</description>
    <graphicData>

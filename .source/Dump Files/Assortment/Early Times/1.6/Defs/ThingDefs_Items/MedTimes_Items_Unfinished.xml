<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="MedTimes_UnfinishedBase" Abstract="True">
    <thingClass>UnfinishedThing</thingClass>
    <category>Item</category>
    <label>unfinished thing</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <useHitPoints>true</useHitPoints>
    <isUnfinishedThing>true</isUnfinishedThing>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <selectable>true</selectable>
    <tradeability>None</tradeability>
    <drawerType>RealtimeOnly</drawerType>
    <!-- For CompForbiddable -->
    <statBases>
		<MaxHitPoints>50</MaxHitPoints>
		<DeteriorationRate>2.0</DeteriorationRate>
		<Beauty>-4</Beauty>
		<Mass>1</Mass> <!-- ingredients are added to this value -->
	</statBases>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
    </comps>
    <alwaysHaulable>true</alwaysHaulable>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
    <thingCategories>
      <li>Unfinished</li>
    </thingCategories>
  </ThingDef>


<!-- Unfinished Papers --> 
  <ThingDef ParentName="MedTimes_UnfinishedBase">
    <defName>MedTimes_UnfinishedPapers</defName>
    <label>unfinished papers</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/MedTimes_UnfinPaper</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<statBases>
      <Flammability>1.0</Flammability>
	</statBases>
    <stuffCategories />
  </ThingDef>
  
  <ThingDef ParentName="MedTimes_UnfinishedBase">

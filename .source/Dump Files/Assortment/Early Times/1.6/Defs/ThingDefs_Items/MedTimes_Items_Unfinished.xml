      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<statBases>
      <Flammability>1.0</Flammability>
	</statBases>
    <stuffCategories />
  </ThingDef>
  
  <ThingDef ParentName="MedTimes_UnfinishedBase">
    <defName>MedTimes_UnfinishedScroll</defName>
    <label>unfinished book</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/MedTimes_UnfinScroll</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<statBases>
      <Flammability>1.0</Flammability>
	</statBases>
    <stuffCategories />
  </ThingDef>

</Defs>
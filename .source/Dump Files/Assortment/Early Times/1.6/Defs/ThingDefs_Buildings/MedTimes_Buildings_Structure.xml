		<description>A simple wooden gate. Must be manually opened and closed, which slows people down.</description>
		<graphicData>
			<texPath>Things/Building/Door/PrimitiveDoor/DoorLog_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
			<drawSize>(1.3,1.3)</drawSize>
			<linkFlags>
				<li>Fences</li>
				<li>Wall</li>
				<li>Rock</li>
			</linkFlags>
		</graphicData>
		<uiIconPath>Things/Building/Door/PrimitiveDoor/DoorLog_MenuIcon</uiIconPath>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<WorkToBuild>135</WorkToBuild>
		</statBases>
		<building>
			<relatedBuildCommands>
				<li>PenMarker</li>
				<li>MedTimes_Fencing</li>
			</relatedBuildCommands>

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="MedTimes_BenchBase">
    <defName>MedTimes_PersonalComputer</defName>
    <label>Personal Computer</label>
    <description>A personal computer for all your gaming needs. It even comes with several of the most popular games pre-installed. It's a relaxing and fun hobby for a certain kind of person.</description>
    <graphicData>
      <texPath>Things/Building/Joy/PersonalComputer/MedTimes_PersonalComputer</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3,1)</drawSize>	
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>	
    <size>(3,1)</size>	
    <fillPercent>0.5</fillPercent>	
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>	
    <rotatable>true</rotatable>
    <statBases>	
      <MaxHitPoints>100</MaxHitPoints>	  
      <Flammability>1.0</Flammability>
      <MarketValue>2350</MarketValue>
      <JoyGainFactor>1.65</JoyGainFactor>	
      <Mass>10</Mass>	  
      <Beauty>60</Beauty>	  
    </statBases>
    <building><joyKind>Gaming_Dexterity</joyKind></building>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>200</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
    </comps>
	<tradeTags>
		<li>Television</li>
		<li>Exotic</li>
		<li>Joy</li>			
	</tradeTags>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
      <li>RewardStandardLowFreq</li>
    </thingSetMakerTags>
  </ThingDef>   

</Defs>
    <!-- wax stuff -->
    <ThingDef ParentName="MedTimes_FlameMeditationBase">
        <defName>MedTimes_Candle</defName>
        <label>Candle</label>
        <description>A small wax stick with cloth for lighting surroundings. A medieval style lighting solution.</description>
        <graphicData>
            <texPath>Things/Building/Temperature/Waxen/Candle</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawRotated>false</drawRotated>
        </graphicData>
        <rotatable>false</rotatable>
        <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
        <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
        <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
        <clearBuildingArea>false</clearBuildingArea>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <pathCost>4</pathCost>
        <constructEffect>ConstructWood</constructEffect>
        <tickerType>Normal</tickerType>
        <fillPercent>0.15</fillPercent>
        <costList>
            <Cloth>1</Cloth>
            <MedTimes_Wax>4</MedTimes_Wax>
        </costList>
        <statBases>
            <Beauty>0</Beauty>
            <MaxHitPoints>20</MaxHitPoints>
            <WorkToBuild>100</WorkToBuild>
            <Flammability>0</Flammability>
            <Mass>0.2</Mass>
            <MeditationFocusStrength>0.0</MeditationFocusStrength>
        </statBases>
        <building>
            <isEdifice>false</isEdifice>
        </building>
        <comps>
            <li Class="CompProperties_Refuelable">
				<fuelConsumptionRate>0.5</fuelConsumptionRate>
				<fuelCapacity>4.0</fuelCapacity>
                <initialFuelPercent>1</initialFuelPercent>
				<fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
                <fuelFilter>
                    <thingDefs>
                        <li>MedTimes_Wax</li>
                    </thingDefs>
                </fuelFilter>
				<showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
            </li>
            <li Class="CompProperties_HeatPusher">
                <compClass>CompHeatPusherPowered</compClass>
                <heatPerSecond>0.5</heatPerSecond>
                <heatPushMaxTemperature>20</heatPushMaxTemperature>
            </li>
            <li Class="CompProperties_Glower">
                <glowRadius>7</glowRadius>
                <glowColor>(252,187,113,0)</glowColor>
            </li>
            <li Class="EarlyTimes.CompProperties_FireOverlayFlickable">
                <fireSize>0.15</fireSize>
				<offset>(0,0,0.25)</offset>
            </li>
        </comps>
        <researchPrerequisites>
            <li>MedTimes_ContainedFire</li>
            <li>MedTimes_WaxUsage</li>
        </researchPrerequisites>
    </ThingDef>
	

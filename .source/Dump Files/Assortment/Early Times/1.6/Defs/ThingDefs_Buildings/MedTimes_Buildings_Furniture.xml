	<ThingDef ParentName="MedTimes_NoQualityBaseTag" Name="EarlyTimes_Bedbase" Abstract="True">
        <thingClass>Building_Bed</thingClass>
        <graphicData>
            <graphicClass>Graphic_Multi</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <stuffCategories>
            <li>Fabric</li>
            <li>Leathery</li>
        </stuffCategories>
        <statBases>
            <BedRestEffectiveness>0.9</BedRestEffectiveness>
            <Comfort>0.5</Comfort>
            <ImmunityGainSpeedFactor>1.05</ImmunityGainSpeedFactor>
            <SurgerySuccessChanceFactor>1</SurgerySuccessChanceFactor>
        </statBases>
        <comps>
            <li Class="CompProperties_AffectedByFacilities">
                <linkableFacilities>
                    <li>EndTable</li>
                    <li>Dresser</li>
                    <li>MedicalToolCabinet</li>
                </linkableFacilities>
            </li>
            <li Class="CompProperties_AssignableToPawn">
                <drawAssignmentOverlay>false</drawAssignmentOverlay>
                <compClass>CompAssignableToPawn_Bed</compClass>
            </li>
        </comps>
        <building>
            <bed_showSleeperBody>True</bed_showSleeperBody>
            <bed_healPerDay>3</bed_healPerDay>
        </building>
        <fillPercent>0.2</fillPercent>
        <staticSunShadowHeight>0.17</staticSunShadowHeight>
        <castEdgeShadows>True</castEdgeShadows>
        <altitudeLayer>Building</altitudeLayer>
        <drawGUIOverlay>true</drawGUIOverlay>
        <passability>PassThroughOnly</passability>
        <socialPropernessMatters>true</socialPropernessMatters>
        <defaultPlacingRot>South</defaultPlacingRot>
        <canOverlapZones>false</canOverlapZones>
        <techLevel>Neolithic</techLevel>
        <researchPrerequisites>
            <li>TS_PrimitiveFurniture</li>
            <li>TS_PrimitiveButchering</li>
        </researchPrerequisites>
    </ThingDef>


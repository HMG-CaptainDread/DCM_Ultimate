<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- Damage amount is handled in a Harmony patch -->
	<Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>[HLX] ReGrowth - Boreal Forest Expansion</li>
		  <li>ReGrowth: Boreal Forest Expansion</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="RG-BF_Plant_Potokus"]/plant/sowResearchPrerequisites</xpath>
				<value>
					<sowResearchPrerequisites>
						<li>TS_FungusDom</li>
					</sowResearchPrerequisites>
				</value>
			</li>
		</operations>
	</match>
  </Operation>

</Patch>
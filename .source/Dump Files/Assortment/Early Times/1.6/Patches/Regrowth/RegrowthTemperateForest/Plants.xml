<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- Damage amount is handled in a Harmony patch -->
	<Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>[HLX] ReGrowth - Temperate Forest Expansion</li>
		  <li>ReGrowth: Temperate Forest Expansion</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="RG-TF_Plant_Cibarius"]/plant/sowResearchPrerequisites</xpath>
				<value>
					<sowResearchPrerequisites>
						<li>TS_FungusDom</li>
					</sowResearchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="RG-TF_Plant_MountainDandelion"]/plant</xpath>
				<value>
					<sowResearchPrerequisites>
						<li>TS_Botany</li>
					</sowResearchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="RG-TF_Plant_NordicDandelion"]/plant</xpath>
				<value>
					<sowResearchPrerequisites>
						<li>TS_Botany</li>
					</sowResearchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="RG-TF_Plant_Cathedralis"]/plant</xpath>
				<value>
					<sowResearchPrerequisites>
						<li>TS_Botany</li>
					</sowResearchPrerequisites>
				</value>
			</li>
		</operations>
	</match>
  </Operation>

</Patch>
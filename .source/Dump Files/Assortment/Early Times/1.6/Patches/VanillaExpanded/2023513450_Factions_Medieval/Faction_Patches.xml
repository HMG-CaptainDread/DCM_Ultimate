<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Vanilla Factions Expanded - Medieval</li>
		<li>Vanilla Factions Expanded - Medieval (Fork)</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[@Name="VFEM_MedievalPawnKindBase"]/invFoodDef</xpath>		
				<value>
					<invFoodDef>MedTimes_MealJerkedMeats</invFoodDef>
				</value>
			</li>
			
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="VFEM_KnightInBlack"]/invFoodDef</xpath>		
				<value>
					<invFoodDef>MedTimes_MealJerkedMeats</invFoodDef>
				</value>
			</li>
			

		</operations>
	</match>
  </Operation>
  
</Patch>





<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Vanilla Fishing Expanded - Fishing Treasures AddOn</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/VCE_Fishing.FishDef[defName = "VCEF_SpecialVWE_Throwing_Rocks"]</xpath>		
			</li>	
			<li Class="PatchOperationRemove">
				<xpath>/Defs/VCE_Fishing.FishDef[defName = "VCEF_SpecialVWE_Gun_Flintlock"]</xpath>		
			</li>
		</operations>
	</match>
  </Operation>
  
</Patch>





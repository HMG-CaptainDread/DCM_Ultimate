<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Vanilla Furniture Expanded - Art</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Res_Obelisk"]</xpath>		
				<value>
					<prerequisites>
						<li>TS_PrimitiveStonecutting</li>
					</prerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Res_Pottery"]</xpath>		
				<value>
					<prerequisites>
						<li>MedTimes_AdobeBricks</li>
					</prerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Res_WallCarvings"]</xpath>		
				<value>
					<prerequisites>
						<li>TS_PrimitiveStonecutting</li>
						<li>TS_Shelter</li>
					</prerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Res_ArtfulDisplay"]</xpath>		
				<value>
					<prerequisites>
						<li>MedTimes_BasicFurniture</li>
						<li>MedTimes_BasicArt</li>
					</prerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Res_Statues"]</xpath>		
				<value>
					<prerequisites>
						<li>TS_PrimitiveStonecutting</li>
						<li>MedTimes_BasicArt</li>
					</prerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Res_Monuments"]</xpath>		
				<value>
					<prerequisites>
						<li>Stonecutting</li>
						<li>MedTimes_Art</li>
					</prerequisites>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Res_Rugs"]</xpath>		
				<value>
					<prerequisites>
						<li>CarpetMaking</li>
					</prerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ResearchProjectDef[defName = "VFE_Wallpaper"]</xpath>		
				<value>
					<prerequisites>
						<li>TS_Shelter</li>
						<li>MedTimes_Art</li>
					</prerequisites>
				</value>
			</li>
			
			
		</operations>
	</match>
  </Operation>
  
</Patch>





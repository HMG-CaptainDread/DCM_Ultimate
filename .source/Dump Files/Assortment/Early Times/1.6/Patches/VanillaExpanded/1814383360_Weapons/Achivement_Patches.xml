<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Vanilla Weapons Expanded</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<!-- cannot crat throqing rock out of jade - current rocks are "rock patchleather" -->
			<li Class="PatchOperationRemove">
				<xpath>/Defs/AchievementsExpanded.AchievementDef[defName = "poshRockLobber"]</xpath>	
			</li>
			
			<!-- cannot crat throqing rock out of jade -  -->
			<li Class="PatchOperationReplace">
				<xpath>/Defs/AchievementsExpanded.AchievementDef[defName = "napoleonBonaparty"]/tracker/def</xpath>			
				<value>
					<def>MedTimes_Ranged_Musket</def>
				</value>
			</li>
			
		</operations>
	</match>
  </Operation>
  
</Patch>





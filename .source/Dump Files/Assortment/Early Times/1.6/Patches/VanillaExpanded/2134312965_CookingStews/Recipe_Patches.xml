<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Vanilla Cooking Expanded - Stews</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "VCE_CookStewSimple"
								   or defName = "VCE_CookStewFine"
								   or defName = "VCE_CookStewLavish"]/recipeUsers</xpath>		
				<value>
					<li>MedTimes_FoodPrepStation</li>
				</value>
			</li>			
			
		</operations>
	</match>
  </Operation>
  
</Patch>





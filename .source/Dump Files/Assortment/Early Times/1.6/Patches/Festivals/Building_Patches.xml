<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Medieval Festival</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VBY_Festival_Game"]</xpath>	
				<value>
					<researchPrerequisites>
						<li>TS_BasicGames</li>
					</researchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VBY_Medieval_Dice"]</xpath>	
				<value>
					<researchPrerequisites>
						<li>TS_SimpleGames</li>
					</researchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VBY_Festival_Tent"]</xpath>	
				<value>
					<researchPrerequisites>
						<li>MedTimes_MedievalTailoring</li>
						<li>TS_Shelter</li>
					</researchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VBYFeast_Table"]</xpath>	
				<value>
					<researchPrerequisites>
						<li>MedTimes_BasicFurniture</li>
					</researchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="VBY_Spit_Roast"]</xpath>	
				<value>
					<researchPrerequisites>
						<li>MedTimes_BasicCooking</li>
					</researchPrerequisites>
				</value>
			</li>
			
		</operations>
	</match>
  </Operation>
    
</Patch>
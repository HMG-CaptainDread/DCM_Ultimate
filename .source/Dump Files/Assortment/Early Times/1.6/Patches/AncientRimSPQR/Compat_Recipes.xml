<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Ancient Rim - SPQRim</li>
            <li>Ancient Rim - SPQRim (Continued)</li>
        </mods>		
        <match Class="PatchOperationSequence">
            <operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName = "AR_FryMeat"]/researchPrerequisite</xpath>
					<value>
						<researchPrerequisite>MedTimes_BasicCooking</researchPrerequisite>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName = "AR_FryEggs"]/researchPrerequisite</xpath>
					<value>
						<researchPrerequisite>MedTimes_BasicCooking</researchPrerequisite>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/RecipeDef[defName = "AR_MakeSalad"]/researchPrerequisite</xpath>
					<value>
						<researchPrerequisite>MedTimes_BasicCooking</researchPrerequisite>
					</value>
				</li>
			</operations>
		</match>
    </Operation>
	
</Patch>				
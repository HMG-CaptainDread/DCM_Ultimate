<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Doors Expanded</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/DoorsExpanded.DoorExpandedDef[defName = "PH_DoorDouble"]</xpath>		
				<value>
					<researchPrerequisites>
						<li>MedTimes_ReinforcedStructure</li>
					</researchPrerequisites>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/DoorsExpanded.DoorExpandedDef[defName = "PH_DoorTriple"]</xpath>		
				<value>
					<researchPrerequisites>
						<li>MedTimes_ReinforcedStructure</li>
					</researchPrerequisites>
				</value>
			</li>
		</operations>
	</match>
  </Operation>
  
</Patch>





<?xml version="1.0" encoding="utf-8" ?>

<Patch>

  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Steam World</li>
		  <li>Steam World (Continued)</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/RecipeDef[defName="MedTimes_MakeChemfuelCoal"]</xpath>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>/Defs/RecipeDef[defName="MedTimes_MakeCoalFromWood"]</xpath>
			</li>
		</operations>
	</match>
  </Operation>
  
	
	
</Patch>
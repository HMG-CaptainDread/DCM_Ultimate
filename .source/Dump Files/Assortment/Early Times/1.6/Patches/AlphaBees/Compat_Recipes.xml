<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Bees</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
	<li Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="RB_CentrifugeAnyComb"]</xpath>
		<value>
			<researchPrerequisite>RB_BasicBeekeeping</researchPrerequisite>
		</value>
	</li>
	<li Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="RB_CentrifugeAnyComb"]</xpath>
		<value>
			<recipeUsers>
				<li>RB_Beekeeping_Workbench_Building</li>
			</recipeUsers>
		</value>
	</li>
	
	<li Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="RB_CreateTier4Research"]/ingredients</xpath>
		<value>
			<li>
				<filter>
					<thingDefs>
						<li>MedTimes_Resource_PaperSheets</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</value>
	</li>
	<li Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="RB_CreateTier4Research"]/fixedIngredientFilter</xpath>
		<value>
			<thingDefs>
				<li>MedTimes_Resource_PaperSheets</li>
			</thingDefs>
		</value>
	</li>
	
	<li Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="RB_CreateTier5Research"]/ingredients</xpath>
		<value>
			<li>
				<filter>
					<thingDefs>
						<li>MedTimes_Resource_PaperSheets</li>
					</thingDefs>
				</filter>
				<count>40</count>
			</li>
		</value>
	</li>
	<li Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="RB_CreateTier5Research"]/fixedIngredientFilter</xpath>
		<value>
			<thingDefs>
				<li>MedTimes_Resource_PaperSheets</li>
			</thingDefs>
		</value>
	</li>
			</operations>
		</match>
	</Operation>
	
</Patch>
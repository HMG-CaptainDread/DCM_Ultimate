<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Bees</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="RB_SimpleHoney"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[defName="RB_LavishHoney"]</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[defName="RB_CookHoneyMealSimple"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[defName="RB_CookHoneyMealSimpleBulk"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[defName="RB_CookHoneyMealFine"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[defName="RB_CookHoneyMealFineBulk"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[defName="RB_CookHoneyMealLavish"]</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/RecipeDef[defName="RB_CookHoneyMealLavishBulk"]</xpath>
				</li>
			</operations>
		</match>
	</Operation>
	
	
</Patch>
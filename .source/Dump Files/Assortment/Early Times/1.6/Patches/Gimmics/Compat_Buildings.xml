<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Gimmicks</li>
		  <li>Gimmicks (Continued)</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
		<li Class="PatchOperationRemove">
			<xpath>/Defs/ThingDef[defName="Gimmicks_TableCrafting"]/recipes</xpath>
		</li>
		<li Class="PatchOperationReplace">
			<xpath>/Defs/ThingDef[defName="Gimmicks_TableCrafting"]/researchPrerequisites</xpath>
			<value>
				<researchPrerequisites>
					<li>MedTimes_Toolworking</li>
				</researchPrerequisites>
			</value>
		</li>
		</operations>
	</match>
  </Operation>

</Patch>





<?xml version="1.0" encoding="utf-8" ?>

<Patch>
	<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Simple Chains: Lumber</li>
        </mods>		
        <match Class="PatchOperationSequence">
            <operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="Owl_GreenLumber"]</xpath>
					<value>    
						<tickerType>Rare</tickerType>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="Owl_GreenLumber"]</xpath>
					<value>    
						<comps>
						  <li Class="EarlyTimes.CompProperties_Dryable">
							<defDriesTo>WoodLog</defDriesTo>
							<daysToDry>1.2</daysToDry>
						  </li>
						</comps>
					</value>
				</li>
            </operations>
		</match>
    </Operation>
	
	
	
</Patch>
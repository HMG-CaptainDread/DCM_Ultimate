<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>VGP Garden Canning</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "CanningFactory"]/comps/li[@Class="CompProperties_Refuelable"]/fuelConsumptionRate</xpath>		
				<value>
					<fuelConsumptionRate>20</fuelConsumptionRate>
				</value>
			</li>
			
		</operations>
	</match>
  </Operation>
    
</Patch>
<?xml version="1.0" encoding="utf-8"?>
<Patch>
  
  <Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>VGP Soylent Production</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ResearchProjectDef[defName="soylentmaking"]/prerequisites</xpath>
				<value>
					<prerequisites>
					  <li>NutrientPaste</li>
					  <li>MedTimes_DriedFood</li>
					</prerequisites>
				</value>				
			</li>
			
		</operations>
	</match>
  </Operation>
    
</Patch>
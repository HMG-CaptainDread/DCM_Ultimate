      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
        <!--li>IndustrialMilitaryBasic</li-->
		<li>SteamworldMilitary</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
	<generateCommonality>0.5</generateCommonality>
	<thingCategories>
      <li>ApparelMisc</li>
    </thingCategories>
	<colorGenerator Class="ColorGenerator_Options">
      <options>
		<li>
          <weight>10</weight>
          <only>(36,84,64)</only><!--> teal <-->
        </li>
		<li>
          <weight>10</weight>
          <only>(114,12,12)</only><!--> bestower red <-->
        </li>
		<li>
          <weight>10</weight>
          <only>(30,30,130)</only><!--> dark blue <-->
        </li>
		<li>
          <weight>10</weight>
          <only>(0,100,200)</only><!--> blue <-->
        </li>
		<li>
          <weight>10</weight>
          <only>(120,200,255)</only><!--> light blue <-->
        </li>
		<li>
          <weight>10</weight>
          <only>(250,250,250)</only><!--> cataphract white <-->
        </li>
      </options>
    </colorGenerator>
    <tradeTags>
      <li>Clothing</li>
      <li>Armor</li>
    </tradeTags>
  </ThingDef>


<Defs>

  <ThingDef ParentName="ApparelMakeableBase" Name="StWoUn_UniformJacketBase" Abstract="True">
    <recipeMaker>
      <researchPrerequisites>
		<li>SteamworldResearch_Uniforms</li>
	  </researchPrerequisites>
    </recipeMaker>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.85</drawSize>

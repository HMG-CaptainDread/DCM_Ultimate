<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Agave-->

<PipeSystem.ProcessDef MayRequire="vanillaexpanded.vplantse">
  <defName>AutoHydroponic_VPEAgaveSm</defName>
  <label>Growing agave</label>
  <description>Grow agave.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 102 agave in 6.4 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>384000 </ticks>
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>RawAgave</thing>
      <count>102</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


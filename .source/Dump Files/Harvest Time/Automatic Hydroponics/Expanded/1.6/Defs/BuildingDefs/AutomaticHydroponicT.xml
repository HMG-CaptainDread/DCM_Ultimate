<?xml version="1.0" encoding="utf-8"?>
<Defs>

<!--Tiny Auto Hydroponic-->
<ThingDef ParentName="BuildingBase">
  <defName>TinyAutoHydroponic</defName>
  <label>Tiny auto hydroponic</label>
  <description>A Tiny automatic hydroponics bay that can grow various plants without the help of a person, instead utilizing mechanical saws and growers. Plants are grown deep in the ground, causing them to be resistant to temperature and toxicity, special gene modding allows plants to grow for a full twenty-four hours a day. 
  
One auto hydroponic is equal to one hydroponics working all day and all night.</description>
  <drawerType>MapMeshAndRealTime</drawerType>
  <graphicData>
    <texPath>EmptyAutoHydroponicTiny</texPath>
    <graphicClass>Graphic_Multi</graphicClass>
    <drawSize>1</drawSize>
  </graphicData>
  <rotatable>false</rotatable>
  <altitudeLayer>FloorEmplacement</altitudeLayer>
  <passability>Standable</passability>
    <size>(1,1)</size>
  <statBases>
    <MaxHitPoints>125</MaxHitPoints>
    <Flammability>0.1</Flammability>
    <Beauty>0</Beauty>
	<WorkToBuild>8000</WorkToBuild>
  </statBases>
  <costList>
    <Steel>100</Steel>
    <ComponentIndustrial>3</ComponentIndustrial>
    <Plasteel>3</Plasteel>
  </costList>
      <researchPrerequisites>
        <li>Fabrication</li>
        <li>Hydroponics</li>
      </researchPrerequisites>
    <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
  <comps>
    <li Class="PipeSystem.CompProperties_AdvancedResourceProcessor">
      <processes>
        <!--Base Game-->
        <li>AutoHydroponic_RiceT</li>
        <li>AutoHydroponic_HealrootT</li>
        <li>AutoHydroponic_PotatoT</li>
        <li MayRequire="ludeon.rimworld.ideology">AutoHydroponic_NutrifungusT</li>
        <li>AutoHydroponic_HayT</li>
        <li>AutoHydroponic_BerriesT</li>
        <li>AutoHydroponic_FibercornT</li>
	    	<li>AutoHydroponic_HopsT</li>
		    <li>AutoHydroponic_PsychoidT</li>
		    <li>AutoHydroponic_SmokeT</li>
		    <li>AutoHydroponic_DyeT</li>
		    <li>AutoHydroponic_CottonT</li>
		    <li>AutoHydroponic_CornT</li>
			<li>AutoHydroponic_DevilstrandT</li>
        <!--Paste and Chemfuel-->
        <li>AutoHydroponic_NutrientPasteT</li>
        <li>AutoHydroponic_ChemfuelT</li>
        <li MayRequire="vanillaexpanded.vchemfuele">AutoHydroponic_ChemfuelNetworkT</li>
        <li MayRequire="vanillaexpanded.vnutriente">AutoHydroponic_NutrientPasteNetworkT</li>
        <!--Giant Crops-->
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCBellPepperT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCCabbageT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCCarrotT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCCauliflowerT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCEggplantT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCLettuceT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCOnionT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCPineappleT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCPumpkinT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCTomatoT</li>
        <li MayRequire="lingluo.giantcrop">AutoHydroponic_LGCWatermelonT</li>
        <!--Hemofungus-->
        <li MayRequire="nosleep.hemofungus">AutoHydroponic_HFHemofungusT</li>
        <!--Meatleaf-->
        <li MayRequire="argon.meatleaf">AutoHydroponic_MLMeatleafT</li>
        <!--GroThing-->
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTSteelT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTGoldT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTPlasteelT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTJadeT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTSilverT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTUraniumT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTWoodT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTPatchLeatherT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTLightLeatherT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTPlainLeatherT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTHeavyLeatherT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTSynthreadT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTHyperweaveT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTComponentT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTAdvancedComponentT</li>
        <li MayRequire="dandman.grothingthings">AutoHydroponic_GTNeutroshroomT</li>
        <!--Farmable Neutroamine-->
        <li MayRequire="kongkim.farmableneutroamine">AutoHydroponic_FNNeutroamineT</li>
        <!--Neutroamine Network-->
        <li MayRequire="vanillaracesexpanded.android,kongkim.farmableneutroamine">AutoHydroponic_NNNeutroamineT</li>
		<!--Vanilla Plants Expanded-->
		<li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPEAgaveT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPEOnionT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPEPumpkinT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPEPepperT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPEPeasT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPEEggplantT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPECabbageT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPEBeetT</li>
        <li MayRequire="vanillaexpanded.vplantse">AutoHydroponic_VPETomatoT</li>
        <!--Erin's Hot Drinks-->
        <li MayRequire="erin.hotdrink">AutoHydroponic_EHDCoffeeT</li>
        <li MayRequire="erin.hotdrink">AutoHydroponic_EHDTeaT</li>
      </processes>
      <progressBarOffset>(0,0,-0.5)</progressBarOffset>
      <inProgressTexture>WorkingAutoHydroponicTiny</inProgressTexture>
      <finishedTexture>EmptyAutoHydroponicTiny</finishedTexture>
	  <notWorkingKey>EmptyAutoHydroponic</notWorkingKey>
    </li>
    <li Class="PipeSystem.CompProperties_GlowerOnProcess">
      <glowRadius>4</glowRadius>
      <glowColor>(245, 192, 226)</glowColor>
    </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>200</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Breakdownable"/>
      <li Class="PipeSystem.CompProperties_Resource" MayRequire="vanillaexpanded.vnutriente">
        <pipeNet MayRequire="vanillaexpanded.vnutriente">VNPE_NutrientPasteNet</pipeNet>
      </li>
      <li Class="PipeSystem.CompProperties_Resource" MayRequire="vanillaexpanded.vchemfuele">
        <pipeNet MayRequire="vanillaexpanded.vchemfuele">VCHE_ChemfuelNet</pipeNet>
      </li>
      <li Class="PipeSystem.CompProperties_Resource" MayRequire="vanillaracesexpanded.android,kongkim.farmableneutroamine">
        <pipeNet MayRequire="vanillaracesexpanded.android,kongkim.farmableneutroamine">VREA_NeutroamineNet</pipeNet>
      </li>
  </comps>
  <inspectorTabs>
    <li>PipeSystem.ITab_Processor</li>
  </inspectorTabs>
  <tickerType>Rare</tickerType>
  <designationCategory>Production</designationCategory>
  <building>
    <isEdifice>false</isEdifice>
  </building>
  <clearBuildingArea>false</clearBuildingArea>
</ThingDef>
</Defs>

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!--Dandelions-->

<PipeSystem.ProcessDef>
  <defName>FloralHydroponic_DandelionsSm</defName>
  <label>Growing dandelions</label>
  <description>Grow dandelions.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow dandelions</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>120000</ticks> 
  <ingredients>
  <li>
    <thing>WoodLog</thing>
    <countNeeded>0</countNeeded>
  </li>
</ingredients>
  <results>
    <li>
      <thing>Hay</thing>
      <count>40</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
</PipeSystem.ProcessDef>


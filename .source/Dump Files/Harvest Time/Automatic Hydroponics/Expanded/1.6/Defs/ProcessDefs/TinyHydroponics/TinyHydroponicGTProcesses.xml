<Defs>

<!--Steel-->

<PipeSystem.ProcessDef MayRequire="dandman.grothingthings">
  <defName>AutoHydroponic_GTSteelT</defName>
  <label>Growing steel</label>
  <description>Grow steel.</description>
  <spawnOnInteractionCell>true</spawnOnInteractionCell>
  <labelOverride>Grow 19 steel in 6.6 days</labelOverride>
  <priorityInBillList>0</priorityInBillList>
  <ticks>396000</ticks>
  <ingredients>
    <li>
      <thing>WoodLog</thing>
      <countNeeded>0</countNeeded>
    </li>
  </ingredients>
  <results>
    <li>
      <thing>Steel</thing>
      <count>19</count>
    </li>
  </results>
  <autoExtract>true</autoExtract>
  <sustainerWhenWorking>true</sustainerWhenWorking>
  <sustainerDef>Recipe_CookMeal</sustainerDef>
  <researchPrerequisites>
    <li>grothingthings</li>
  </researchPrerequisites>
</PipeSystem.ProcessDef>


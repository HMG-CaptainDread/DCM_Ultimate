<?xml version="1.0" encoding="utf-8" ?>

<Patch>	

	<Operation Class="PatchOperationFindMod">
	  <mods>
		  <li>Early Times</li>
		<li>Early Times (Continued)</li>
	  </mods>
	  <match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TerraformTech.TerrainTerraformRuleSet[defName="Terraform_BasicSoilRemoval"]/rules/li[patchTag="SoilFromGravel"]/resultList</xpath>
				<value>
					<Crushed_Stones>20</Crushed_Stones>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TerraformTech.TerrainTerraformRuleSet[defName="Terraform_BasicSoilRemoval"]/rules/li[patchTag="SoftSandFromSand"]/resultList</xpath>
				<value>
					<Crushed_Stones>20</Crushed_Stones>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TerraformTech.TerrainTerraformRuleSet[defName="Terraform_BasicSoilRemoval"]/rules/li[patchTag="SoilFromSoftSand"]/resultList</xpath>
				<value>
					<Resource_Sand>200</Resource_Sand>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TerraformTech.TerrainTerraformRuleSet[defName="Terraform_BasicAddTerrain"]/rules/li[patchTag="StonyTerrainFromSoil"]/costList</xpath>
				<value>
					<Crushed_Stones>20</Crushed_Stones>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TerraformTech.TerrainTerraformRuleSet[defName="Terraform_BasicAddTerrain"]/rules/li[patchTag="SandFromSoftSand"]/costList</xpath>
				<value>
					<Crushed_Stones>20</Crushed_Stones>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TerraformTech.TerrainTerraformRuleSet[defName="Terraform_BasicAddTerrain"]/rules/li[patchTag="SoftSandFromSoil"]/costList</xpath>
				<value>
					<Resource_Sand>200</Resource_Sand>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/TerraformTech.TerrainTerraformRuleSet[defName="Terraform_BasicAddTerrain"]/rules/li[patchTag="SandFromGravel"]/costList</xpath>
				<value>
					<Resource_Sand>200</Resource_Sand>
				</value>
			</li>
		</operations>
	</match>
  </Operation>
	
</Patch>